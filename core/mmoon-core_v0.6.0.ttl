@base <http://schema.mmoon.org/core#> .

<http://schema.mmoon.org/core#> rdf:type owl:Ontology ;
                         
                         rdfs:label "mmoon" ;
                         
                         rdfs:comment """The Multilingual Morpheme Ontology (MMoOn) is  an ontological model that contains the vocabulary needed to describe the morphological meanings and their represantations in natural languages. 

This is the reference ontology from which various language specific MMoOn Inventories will be derived. 


Version 1.0, 2015""" .



#
#    Object Properties
#





:belongsToLexeme rdf:type owl:ObjectProperty .





:belongsToWord-form rdf:type owl:ObjectProperty .





:consistsOfMorpheme rdf:type owl:ObjectProperty ;
                    
                    rdfs:range :Morpheme .





:hasDerivationalMeaning rdf:type owl:ObjectProperty ;
                        
                        rdfs:domain :MorphologicalMeaning .





:hasGloss rdf:type owl:ObjectProperty .





:hasInflectionalCategory rdf:type owl:ObjectProperty ;
                         
                         rdfs:range :InflectionalCategory ;
                         
                         rdfs:domain :MorphologicalMeaning .





:hasInflectionalMeaning rdf:type owl:ObjectProperty ;
                        
                        rdfs:domain :MorphologicalMeaning .





:hasMeaning rdf:type owl:ObjectProperty ;
            
            rdfs:domain :Morpheme ;
            
            rdfs:range :MorphologicalMeaning .





:hasMorphemicEntry rdf:type owl:ObjectProperty ;
                   
                   rdfs:range :Morpheme ;
                   
                   rdfs:domain :MorphemeInventory .





:hasMorphemicGloss rdf:type owl:ObjectProperty ;
                   
                   rdfs:domain :Morpheme ;
                   
                   rdfs:range :MorphemicGloss ;
                   
                   rdfs:subPropertyOf :hasRepresentation .





:hasMorphemicRepresention rdf:type owl:ObjectProperty ;
                          
                          rdfs:comment "The morphemic form is the canonical form of a morpheme in the morpheme inventory and serves as an entry of the morpheme inventory. It consists of a hyphen and the conventional orthographic representation of it even if it has more than one orthographic surface forms. E.g. the genetive morpheme \"-s\" in English has two orthographic surface forms, 1) for all words not ending on <s> it is apostrophe and <s> and 2) for all words ending on <s> the apostrophe alone renders the genetive morpheme." ;
                          
                          rdfs:domain :Morpheme ;
                          
                          rdfs:range :MorphemicRepresentation ;
                          
                          rdfs:subPropertyOf :hasRepresentation .





:hasOrthographicRepresentation rdf:type owl:ObjectProperty ;
                               
                               rdfs:domain :Morpheme ;
                               
                               rdfs:range :OrthographicRepresentation ;
                               
                               rdfs:subPropertyOf :hasRepresentation .





:hasPhoneticRepresentation rdf:type owl:ObjectProperty ;
                           
                           rdfs:domain :Morpheme ;
                           
                           rdfs:range :PhoneticRepresentation ;
                           
                           rdfs:subPropertyOf :hasRepresentation .





:hasRepresentation rdf:type owl:ObjectProperty .





:hasStem rdf:type owl:ObjectProperty .





:hasWord-FormEntry rdf:type owl:ObjectProperty ;
                   
                   rdfs:domain :MorphemeInventory .





:isHomonymTo rdf:type owl:ObjectProperty ,
                      owl:SymmetricProperty ;
             
             rdfs:range :Morpheme ;
             
             rdfs:domain :Morpheme .





:isPartOf rdf:type owl:ObjectProperty ;
          
          rdfs:range :InflectionalCategory .






#
#    Data properties
#





:language rdf:type owl:DatatypeProperty ;
          
          rdfs:domain :MorphemeInventory ;
          
          rdfs:range xsd:string .





:representation rdf:type owl:DatatypeProperty ;
                
                rdfs:comment "Beschreiben, dass der string je nach Form, auf die sie sich bezieht, auch als morphemische, orthographische oder phonologische notation zu verwenden ist. deshalb, sollte für die morphemische repr. das lateinische alphabet und Bindestrich verwendet werden, für die orthographische die Schrift der jeweiligen sprache und für die phonologische IPA" ;
                
                rdfs:domain :Representation ;
                
                rdfs:range xsd:string .






#
#    Classes
#





:Ablative rdf:type owl:Class ;
          
          rdfs:subClassOf :Case ;
          
          rdfs:comment """The ablative is a case that signals spatial orientation, esp. motion, away from the referent of its carrier.
To the extent that it is grammaticalized in a language, its function may also include other circumstantial case roles. (Christian Lehman, LIDO)""" .





:Accusative rdf:type owl:Class ;
            
            rdfs:subClassOf :Case .





:Active rdf:type owl:Class ;
        
        rdfs:subClassOf :Voice .





:Aktionsart rdf:type owl:Class ;
            
            rdfs:subClassOf :InflectionalCategory .





:Allative rdf:type owl:Class ;
          
          rdfs:subClassOf :Case ;
          
          rdfs:comment "The allative is a case which expresses spatial orientation, esp. motion, to or toward the referent of its carrier. (Christian Lehmann, LIDO)" .





:Antipassive rdf:type owl:Class ;
             
             rdfs:subClassOf :Voice ;
             
             rdfs:comment "The antipassive is a voice of an ergative system which demotes the undergoer (in absolutive function) to an adjunct - e.g. an instrumental or a locative or dative - and promotes the actor (in ergative function) to absolutive function. (Christian Lehmann, LIDO)"@de .





:Base rdf:type owl:Class ;
      
      rdfs:subClassOf :Stem .





:Benefactive rdf:type owl:Class ;
             
             rdfs:subClassOf :Case ;
             
             rdfs:comment "The benefactive (casus benefactivus) is a case which marks the semantic role of beneficiary.(Christian Lehmann, LIDO)" .





:Clusivity rdf:type owl:Class ;
           
           rdfs:subClassOf :InflectionalCategory .





:Comitative rdf:type owl:Class ;
            
            rdfs:subClassOf :Case ;
            
            rdfs:comment "The comitative case on a nominal expression signifies that the referent of that expression is a peripheral participant of a situation that takes part in it in a way analogous to some central participant, usually the actor. (Christian Lehmann, LIDO)" .





:Comparative rdf:type owl:Class ;
             
             rdfs:subClassOf :DegreeOfComparison .





:Compounding rdf:type owl:Class ;
             
             rdfs:subClassOf :WordFormation .





:Conditional rdf:type owl:Class ;
             
             rdfs:subClassOf :Mood .





:Dative rdf:type owl:Class ;
        
        rdfs:subClassOf :Case .





:Declarative rdf:type owl:Class ;
             
             rdfs:subClassOf :Mood .





:Dual rdf:type owl:Class ;
      
      rdfs:subClassOf :Number .





:Equative rdf:type owl:Class ;
          
          rdfs:label "Äquativ"@de ,
                     "Equative"@en ;
          
          rdfs:subClassOf :Case ;
          
          rdfs:comment "a case that expresses likeness or identity to the referent of the NP, thus having the meaning of „as“,“like“, or „in the capacity of“. (Lehmann, LIDO http://linguistik.uni-erfurt.de:8080/lido/Lido)" .





:Exclusive rdf:type owl:Class ;
           
           rdfs:subClassOf :Clusivity .





:Feminine rdf:type owl:Class ;
          
          rdfs:subClassOf :Gender .





:Finite rdf:type owl:Class ;
        
        rdfs:subClassOf :Finiteness .





:Finiteness rdf:type owl:Class ;
            
            rdfs:subClassOf :InflectionalCategory ;
            
            rdfs:comment "The finiteness of a linguistic unit - generally a verb or a clause - is its property of being finite (or not). (Christian Lehmann, LIDO)"@de .





:FirstPerson rdf:type owl:Class ;
             
             rdfs:subClassOf :Person ;
             
             rdfs:comment "The fist person is the value of the category person which prototypically represents the speaker. (Christian Lehmann, LIDO)"@de .





:Future rdf:type owl:Class ;
        
        rdfs:subClassOf :Tense .





:Genitive rdf:type owl:Class ;
          
          rdfs:subClassOf :Case .





:Gerundive rdf:type owl:Class ;
           
           rdfs:subClassOf :NonFiniteVerb .





:Imperative rdf:type owl:Class ;
            
            rdfs:subClassOf :Mood ;
            
            rdfs:comment """The imperative is the mood that encodes directive modality, especially demands, directed towards a speech act participant.
The imperative in the strict sense is of second person. The imperative in the wide sense includes the hortative and the jussive. (Christian Lehmann, LIDO)"""@de .





:Inclusive rdf:type owl:Class ;
           
           rdfs:subClassOf :Clusivity .





:Indicative rdf:type owl:Class ;
            
            rdfs:subClassOf :Mood ;
            
            rdfs:comment "The indicative is the unmarked mood. It is used when no special modal nuance in the clause or sentence is intended. It is the default mood of independent declarative and often also of interrogative sentences.(Christian Lehmann, LIDO)"@de .





:Infinite rdf:type owl:Class ;
          
          rdfs:subClassOf :Finiteness .





:Infinitive rdf:type owl:Class ;
            
            rdfs:subClassOf :NonFiniteVerb .





:Instrumental rdf:type owl:Class ;
              
              rdfs:subClassOf :Case ;
              
              rdfs:comment "The instrumental is a case such that an NP in the instrumental means ‚with (i.e. using) NP’. (Christian Lehmann, LIDO)" .





:Irrealis rdf:type owl:Class ;
          
          rdfs:subClassOf :Mood ;
          
          rdfs:comment "Irrealis is a modality which conveys that the proposition is counterfactual. (Christian Lehmann, LIDO)"@de .





:Irregular rdf:type owl:Class ;
           
           rdfs:subClassOf :Conjugation .





:Locative rdf:type owl:Class ;
          
          rdfs:subClassOf :Case ;
          
          rdfs:comment "The locative is a case that expresses location at the referent of its carrier. (Christian Lehmann, LIDO)" .





