                     rdfs:subClassOf :DeadjectivalAdjective ;
                     
                     rdfs:seeAlso "http://purl.org/linguistics/gold/DiminuativeSize" ;
                     
                     rdfs:comment "An adjective which is derived from an adjective base that denotes a smaller intensity than the adjective base."@en ;
                     
