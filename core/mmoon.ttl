@prefix : <http://mmoon.org/mmoon/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix gold: <http://purl.org/linguistics/gold/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix lexvo: <http://lexvo.org/ontology#> .
@prefix mmoon: <http://mmoon.org/mmoon/> .
@base <http://mmoon.org/mmoon/> .

<http://mmoon.org/mmoon/> rdf:type owl:Ontology ;
                          
                          rdfs:label "Multilingual Morpheme Ontology"@en ;
                          
                          owl:versionInfo "Version 1.0, 2016"@en ;
                          
                          mmoon:definition """The Multilingual Morpheme Ontology (MMoOn) is  an ontological model that contains the vocabulary needed to describe the morphological elements, meanings and representations of natural languages. 

This is the reference ontology from which various language specific MMoOn Inventories can be derived."""@en .


#################################################################
#
#    Annotation properties
#
#################################################################


###  http://mmoon.org/mmoon/definition

mmoon:definition rdf:type owl:AnnotationProperty ;
                 
                 mmoon:definition "A definition for MMoOn vocabulary item (classes, properties, also possibly individuals)."@en ;
                 
                 rdfs:subPropertyOf rdfs:comment ,
                                    skos:definition .



###  http://mmoon.org/mmoon/example

mmoon:example rdf:type owl:AnnotationProperty ;
              
              rdfs:subPropertyOf rdfs:comment .



###  http://mmoon.org/mmoon/externalDefinition

mmoon:externalDefinition rdf:type owl:AnnotationProperty ;
                         
                         mmoon:definition """A reference to a (typically alternative) definition for MMoOn vocabulary items. 

Solely to be used to refer to Web Resources identified by their URI/IRI (which ideally should be Linked Data)."""@en ;
                         
                         rdfs:subPropertyOf rdfs:comment ;
                         
                         rdfs:range rdfs:Resource .



###  http://mmoon.org/mmoon/externalDefinitionDesc

mmoon:externalDefinitionDesc rdf:type owl:AnnotationProperty ;
                             
                             mmoon:definition """A reference to an (typically alternative) definition for MMoOn vocabulary items. 

To be used when no URI/IRI can be provided as
reference point or when additional textual information has to be provided for an URI/IRI reference to be sufficiently specific."""@en ;
                             
                             rdfs:subPropertyOf rdfs:comment ;
                             
                             rdfs:range xsd:string .



###  http://mmoon.org/mmoon/usageNote

mmoon:usageNote rdf:type owl:AnnotationProperty ;
                
                rdfs:subPropertyOf rdfs:comment .



###  http://www.w3.org/2004/02/skos/core#definition

skos:definition rdf:type owl:AnnotationProperty .





#################################################################
#
#    Object Properties
#
#################################################################


###  http://mmoon.org/mmoon/associatedToLanguage

mmoon:associatedToLanguage rdf:type owl:ObjectProperty ;
                           
                           rdfs:range lexvo:Language ;
                           
                           mmoon:definition "States that a morpheme inventory, morph or word is associated to a specific langauge."@en ;
                           
                           rdfs:label "associated to language"@en .



###  http://mmoon.org/mmoon/attachedToRoot

mmoon:attachedToRoot rdf:type owl:ObjectProperty ;
                     
                     rdfs:domain mmoon:Affix ;
                     
                     rdfs:range mmoon:Root ;
                     
                     mmoon:definition "States that an affix is attached to some root."@en ;
                     
                     rdfs:label "attached to root"@en .



###  http://mmoon.org/mmoon/attachedToStem

mmoon:attachedToStem rdf:type owl:ObjectProperty ;
                     
                     rdfs:domain mmoon:Affix ;
                     
                     rdfs:range mmoon:Stem ;
                     
                     mmoon:definition "States that an affix is attached to some stem."@en ;
                     
                     rdfs:label "attached to stem"@en .



###  http://mmoon.org/mmoon/belongsToLexeme

mmoon:belongsToLexeme rdf:type owl:ObjectProperty ;
                      
                      owl:inverseOf mmoon:hasWordform ;
                      
                      rdfs:domain mmoon:Wordform ;
                      
                      rdfs:range mmoon:Lexeme ;
                      
                      mmoon:definition "States that a word-form resource belongs to a lexeme resource."@en ;
                      
                      rdfs:label "belongs to lexeme"@en .



###  http://mmoon.org/mmoon/belongsToWord

mmoon:belongsToWord rdf:type owl:ObjectProperty ;
                    
                    owl:inverseOf mmoon:consistsOfMorph ;
                    
                    rdfs:domain mmoon:Morph ;
                    
                    rdfs:range mmoon:Word ;
                    
                    mmoon:definition "States that a morph belongs to some word."@en ;
                    
                    rdfs:label "belongs to word"@en .



###  http://mmoon.org/mmoon/compoundingRelation

mmoon:compoundingRelation rdf:type owl:ObjectProperty ;
                          
                          rdfs:subPropertyOf mmoon:hasMorphologicalRelationship ;
                          
                          rdfs:domain mmoon:CompoundWord ;
                          
                          rdfs:range mmoon:Compounding ;
                          
                          mmoon:definition "States which kind of compounding morphological relationship applies to a compound word."@en ;
                          
                          rdfs:label "compounding morphological relationship"@en .



###  http://mmoon.org/mmoon/consistsOfAffix

mmoon:consistsOfAffix rdf:type owl:ObjectProperty ;
                      
                      rdfs:subPropertyOf mmoon:consistsOfMorph ;
                      
                      rdfs:domain [ rdf:type owl:Class ;
                                    owl:unionOf ( mmoon:Root
                                                  mmoon:Stem
                                                  mmoon:Word
                                                )
                                  ] ;
                      
                      rdfs:range mmoon:Affix ;
                      
                      mmoon:definition "States that a  word, stem or root consists of some affix."@en ;
                      
                      rdfs:label "consists of affix"@en .



###  http://mmoon.org/mmoon/consistsOfMorph

mmoon:consistsOfMorph rdf:type owl:ObjectProperty ;
                      
                      rdfs:domain mmoon:Word ;
                      
                      rdfs:range mmoon:Morph ;
                      
                      mmoon:definition "States that a word consists of some morph."@en ;
                      
                      rdfs:label "consists of morph"@en .



###  http://mmoon.org/mmoon/consistsOfMorpheme

mmoon:consistsOfMorpheme rdf:type owl:ObjectProperty ;
                         
                         rdfs:domain mmoon:ComplexMorpheme ;
                         
                         rdfs:range [ rdf:type owl:Class ;
                                      owl:unionOf ( mmoon:AtomicMorpheme
                                                    mmoon:EmptyMorpheme
                                                    mmoon:FusionalMorpheme
                                                  )
                                    ] ;
                         
                         mmoon:definition "States that a complex morpheme consists of fusional and/or atomic and/or empty morphemes."@en ;
                         
                         rdfs:label "consist of morpheme"@en .



###  http://mmoon.org/mmoon/consistsOfRoot

mmoon:consistsOfRoot rdf:type owl:ObjectProperty ;
                     
                     rdfs:subPropertyOf mmoon:consistsOfMorph ;
                     
                     rdfs:domain [ rdf:type owl:Class ;
                                   owl:unionOf ( mmoon:Stem
                                                 mmoon:Word
                                               )
                                 ] ;
                     
                     rdfs:range mmoon:Root ;
                     
                     mmoon:definition "States that a stem or word consists of some root."@en ;
                     
                     rdfs:label "consists of root"@en .



###  http://mmoon.org/mmoon/consistsOfStem

mmoon:consistsOfStem rdf:type owl:ObjectProperty ;
                     
                     rdfs:subPropertyOf mmoon:consistsOfMorph ;
                     
                     rdfs:domain [ rdf:type owl:Class ;
                                   owl:unionOf ( mmoon:Stem
                                                 mmoon:Word
                                               )
                                 ] ;
                     
                     rdfs:range mmoon:Stem ;
                     
                     mmoon:definition "States that a word or stem consists of some stem."@en ;
                     
                     rdfs:label "consists of stem"@en .



###  http://mmoon.org/mmoon/consistsOfWord

mmoon:consistsOfWord rdf:type owl:ObjectProperty ;
                     
                     rdfs:domain [ rdf:type owl:Class ;
                                   owl:unionOf ( mmoon:AnalyticWordform
                                                 mmoon:CompoundWord
                                               )
                                 ] ;
                     
                     rdfs:range mmoon:Word ;
                     
                     mmoon:definition "States that an analytic word-form (such as \"more big\" being the comparative form of the adjective \"big\") or compound word consists of other word resources."@en ;
                     
                     rdfs:label "consists of word"@en .



###  http://mmoon.org/mmoon/correspondsToMorpheme

mmoon:correspondsToMorpheme rdf:type owl:ObjectProperty ;
                            
                            owl:inverseOf mmoon:hasRealization ;
                            
                            rdfs:domain mmoon:Morph ;
                            
                            rdfs:range mmoon:Morpheme ;
                            
                            mmoon:definition "States that a given morph corresponds to a certain morpheme."@en ;
                            
                            rdfs:label "corresponds to morpheme"@en .



###  http://mmoon.org/mmoon/derivationalRelation

mmoon:derivationalRelation rdf:type owl:ObjectProperty ;
                           
                           rdfs:subPropertyOf mmoon:hasMorphologicalRelationship ;
                           
                           rdfs:domain mmoon:DerivedWord ;
                           
                           rdfs:range mmoon:Derivation ;
                           
                           mmoon:definition "States which kind of derivational morphological relationship applies to a derived word."@en ;
                           
                           mmoon:usageNote "This property is also used to state that a simple lexeme which cannot be further segmented with respect to word-formation has the morphological relationship mmoon:NoWordFormation."@en ;
                           
                           rdfs:label "derivational morphological relationship"@en .



###  http://mmoon.org/mmoon/forLanguage

mmoon:forLanguage rdf:type owl:ObjectProperty ;
                  
                  rdfs:subPropertyOf mmoon:associatedToLanguage ;
                  
                  rdfs:domain mmoon:MorphemeInventory ;
                  
                  mmoon:definition "States that a MMoOn morpheme inventory is specified for a certain language."@en ;
                  
                  rdfs:label "for language"@en .



###  http://mmoon.org/mmoon/hasAbstractIdentity

mmoon:hasAbstractIdentity rdf:type owl:ObjectProperty ;
                          
                          rdfs:domain [ rdf:type owl:Class ;
                                        owl:unionOf ( mmoon:Meaning
                                                      mmoon:Morpheme
                                                    )
                                      ] ;
                          
                          rdfs:range mmoon:MorphemicGloss ;
                          
                          rdfs:comment "This property states that a morpheme or meaning resource is assigned to one (ore more) morphemic gloss(es)."@en ;
                          
                          rdfs:label "has abstract identity"@en .



###  http://mmoon.org/mmoon/hasMeaning

mmoon:hasMeaning rdf:type owl:ObjectProperty ;
                 
                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( mmoon:Morpheme
                                             mmoon:Word
                                           )
                             ] ;
                 
                 rdfs:range mmoon:Meaning ;
                 
                 rdfs:label "has meaning"@en .



###  http://mmoon.org/mmoon/hasMorphemicEntry

mmoon:hasMorphemicEntry rdf:type owl:ObjectProperty ;
                        
                        rdfs:domain mmoon:MorphemeInventory ;
                        
                        rdfs:range mmoon:Morpheme ;
                        
                        mmoon:definition "States that a morpheme inventory resource has morphemes as morphemic entries, thus building the intension of the morpheme inventory."@en ;
                        
                        rdfs:label "has morphemic entry"@en .



###  http://mmoon.org/mmoon/hasMorphologicalRelationship

mmoon:hasMorphologicalRelationship rdf:type owl:ObjectProperty ;
                                   
                                   rdfs:domain mmoon:Word ;
                                   
                                   rdfs:range mmoon:MorphologicalRelationship ;
                                   
                                   mmoon:definition "Specifies the morphological relationship of a word."@en ;
                                   
                                   rdfs:label "has morphological relationship"@en .



###  http://mmoon.org/mmoon/hasRealization

mmoon:hasRealization rdf:type owl:ObjectProperty ;
                     
                     rdfs:domain mmoon:Morpheme ;
                     
                     rdfs:range mmoon:Morph ;
                     
                     mmoon:definition "States that a morpheme is realized by some morph."@en ;
                     
                     rdfs:label "has realization"@en .



###  http://mmoon.org/mmoon/hasRepresentation

mmoon:hasRepresentation rdf:type owl:ObjectProperty ;
                        
                        rdfs:domain [ rdf:type owl:Class ;
                                      owl:unionOf ( mmoon:Morph
                                                    mmoon:Word
                                                  )
                                    ] ;
                        
                        rdfs:range mmoon:Representation ;
                        
                        mmoon:definition "States that a word or morph has some linguistic representation."@en ;
                        
                        rdfs:label "has representation"@en .



###  http://mmoon.org/mmoon/hasWordform

mmoon:hasWordform rdf:type owl:ObjectProperty ;
                  
                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( mmoon:GrammaticalWord
                                              mmoon:Lexeme
                                            )
                              ] ;
                  
                  rdfs:range mmoon:Wordform ;
                  
                  mmoon:definition "States that (typically) a lexeme resource has an associated word-form resource."@en ;
                  
                  rdfs:label "has wordform"@en .



###  http://mmoon.org/mmoon/inflectionalRelation

mmoon:inflectionalRelation rdf:type owl:ObjectProperty ;
                           
                           rdfs:subPropertyOf mmoon:hasMorphologicalRelationship ;
                           
                           rdfs:domain mmoon:Wordform ;
                           
                           rdfs:range mmoon:Inflection ;
                           
                           mmoon:definition "States which kind of inflectional morphological relationship applies to a word-form."@en ;
                           
                           rdfs:label "inflectional morphological relationship"@en .



###  http://mmoon.org/mmoon/isAllomorphTo

mmoon:isAllomorphTo rdf:type owl:ObjectProperty ,
                             owl:SymmetricProperty ;
                    
                    rdfs:domain mmoon:Morph ;
                    
                    rdfs:range mmoon:Morph ;
                    
                    mmoon:definition "States that a morph is an allomorph to another morph which are both realizations of the same morpheme and which occur in complementary or free distribution."@en ;
                    
                    mmoon:externalDefinitionDesc """http://www-01.sil.org/linguistics/GlossaryOflinguisticTerms/WhatIsAnAllomorph.htm: 
An allomorph is one of two or more complementary morphs which manifest a morpheme in its different phonological or morphological environments."""@en ;
                    
                    rdfs:label "is allomorph to"@en .



###  http://mmoon.org/mmoon/isComposedOf

mmoon:isComposedOf rdf:type owl:ObjectProperty ;
                   
                   rdfs:domain mmoon:CompoundWord ;
                   
                   rdfs:range [ rdf:type owl:Class ;
                                owl:unionOf ( mmoon:GrammaticalWord
                                              mmoon:Lexeme
                                            )
                              ] ;
                   
                   mmoon:definition "States of which lexemes or grammatical words the compound word is composed."@en ;
                   
                   rdfs:label "is composed of"@en .



###  http://mmoon.org/mmoon/isDerivedFrom

mmoon:isDerivedFrom rdf:type owl:ObjectProperty ;
                    
                    rdfs:domain mmoon:DerivedWord ;
                    
                    rdfs:range [ rdf:type owl:Class ;
                                 owl:unionOf ( mmoon:GrammaticalWord
                                               mmoon:Lexeme
                                             )
                               ] ;
                    
                    mmoon:definition "States from which lexeme or grammatical word the derived word is derived."@en ;
                    
                    rdfs:label "is derived from"@en .



###  http://mmoon.org/mmoon/isHomonymTo

mmoon:isHomonymTo rdf:type owl:ObjectProperty ,
                           owl:SymmetricProperty ;
                  
                  rdfs:domain mmoon:Morph ;
                  
                  rdfs:range mmoon:Morph ;
                  
                  mmoon:definition "States that a morph is homonym to another morph, i.e. although sharing the same orthographic and/or morphemic representation both morphs encode different meanings and thus realize different morphemes."@en ;
                  
                  rdfs:label "is homonym to"@en .



###  http://mmoon.org/mmoon/partOfLanguage

mmoon:partOfLanguage rdf:type owl:ObjectProperty ;
                     
                     rdfs:subPropertyOf mmoon:associatedToLanguage ;
                     
                     rdfs:domain [ rdf:type owl:Class ;
                                   owl:unionOf ( mmoon:Morph
                                                 mmoon:Word
                                               )
                                 ] ;
                     
                     mmoon:definition "States that a morph or word resources is a linguistic entity as part of a certain language."@en ;
                     
                     rdfs:label "part of language"@en .





#################################################################
#
#    Data properties
#
#################################################################


###  http://mmoon.org/mmoon/morphemicRepresentation

mmoon:morphemicRepresentation rdf:type owl:DatatypeProperty ;
                              
                              rdfs:domain mmoon:Representation ;
                              
                              rdfs:range xsd:string ;
                              
                              mmoon:definition """The morphemic representation of a morph.
It includes:
a) marking of the prefix  boundary with a hyphen, eg. prefix-
b) marking of the suffix boundary with a hyphen, e.g. -suffix
c) marking of the infix boundry inclosed in angled brackets, e.g. <infix>
d) marking of the circumfix boundary by setting them off by angled brackets, e.g. circumfix prefix><circumfix suffix
e) marking of the transfix and simulfix boundary with a backslash, e.g. \\transfix or simulfix
g) marking of the zero morph and empty morph according to the position they take, e.g. in prefix position: zero/empty morph- , in suffix position: -zero/empty morph

The morphemic representation consists of a morphemic boundary marking and the conventional orthographic representation of it. It differs from the orthographic representation in that it is marked by one of the typographic conventions a) - g). It differs from the morphemic gloss in that it renders the orthographic representation of the morph instead of the corresponding meaning glosses. I.e. for the English genetive \"-s\" morph the three representations are as follows:

orthographic rep.: \"s\"
morphemic rep.: \"-s\"
morphemic gloss: \"GEN\""""@en ;
                              
                              rdfs:label "morphemic representation"@en .



###  http://mmoon.org/mmoon/orthographicRepresentation

mmoon:orthographicRepresentation rdf:type owl:DatatypeProperty ;
                                 
                                 rdfs:domain mmoon:Representation ;
                                 
                                 rdfs:range xsd:string ;
                                 
                                 mmoon:definition "Is the representation adhering to the charactersof the writing system (graphemes) according to the language of the MMoOn inventory."@en ;
                                 
                                 rdfs:label "orthographic representation"@en .



###  http://mmoon.org/mmoon/phoneticRepresentation

mmoon:phoneticRepresentation rdf:type owl:DatatypeProperty ;
                             
                             rdfs:domain mmoon:Representation ;
                             
                             rdfs:range xsd:string ;
                             
                             mmoon:definition "Is the phonetic representation according to the IPA. symbols."@en ;
                             
                             rdfs:label "phonetic representation"@en .



###  http://mmoon.org/mmoon/transliteration

mmoon:transliteration rdf:type owl:DatatypeProperty ;
                      
                      mmoon:definition "This property renders orthographic representations of the writing system of the language of the morpheme inventory in symbols of another writing system, which ideally adheres to the latin alphabet."@en ;
                      
                      mmoon:externalDefinition <https://en.wikipedia.org/wiki/ISO_basic_Latin_alphabet> ;
                      
                      mmoon:usageNote "A reference to the mapping relation of the two writing system involved is highly recommended."@en ;
                      
                      rdfs:label "transliteration"@en .





#################################################################
#
#    Classes
#
#################################################################


###  http://lexvo.org/ontology#Language

lexvo:Language rdf:type owl:Class ;
               
               rdfs:label "Language"@en .



###  http://mmoon.org/mmoon/ActionNoun

mmoon:ActionNoun rdf:type owl:Class ;
                 
                 rdfs:subClassOf mmoon:DeverbalNoun ;
                 
                 mmoon:definition "A noun which is derived from a verb stem denoting the action of the deriving verb stem."@en ;
                 
                 mmoon:example "Eng. \"to arrive\"(V) --> \"arrival\"(N)."@en ;
                 
                 mmoon:externalDefinitionDesc """Christian Lehmann, under \"action noun\" at http://linguistik.uni-erfurt.de:8080/lido/Lido :
An action noun is a non-oriented verbal noun (nomen substantivum), i.e. one which designates the same kind of thing as a (finite) verb, viz. a situation core, except that this is hypostatized as an entity. An action noun is a non-oriented verbal noun (nomen substantivum), i.e. one which designates the same kind of thing as a (finite) verb, viz. a situation core, except that this is hypostatized as an entity."""@en ;
                 
                 rdfs:label "Action noun"@en .



###  http://mmoon.org/mmoon/AdjectivalInflection

mmoon:AdjectivalInflection rdf:type owl:Class ;
                           
                           rdfs:subClassOf mmoon:Inflection ;
                           
                           mmoon:definition "Is the process of generating word-forms of adjectival stems."@en ;
                           
                           rdfs:label "Adjectival inflection"@en .



###  http://mmoon.org/mmoon/AdjectiveCompound

mmoon:AdjectiveCompound rdf:type owl:Class ;
                        
                        rdfs:subClassOf mmoon:Compounding ;
                        
                        mmoon:definition "An adjective compound is an adjective formed by composition of an adjective stem and another stem."@en ;
                        
                        mmoon:example "Eng.: \"man-eating\"(Adj) = \"man\"(N) + \"eating\"(Adj)."@en ;
                        
                        rdfs:label "Adjective compound"@en .



###  http://mmoon.org/mmoon/AdjectiveNoun

mmoon:AdjectiveNoun rdf:type owl:Class ;
                    
                    rdfs:subClassOf mmoon:Conversion ;
                    
                    mmoon:definition "An adjective noun is a noun which is fomed by conversion from an adjective and which entails no further derivational meaning except the entailed wordclass-change."@en ;
                    
                    mmoon:example "Engl.: \"old\"(Adj) --> \"the old\"(N)."@en ;
                    
                    rdfs:label "Adjective noun"@en .



###  http://mmoon.org/mmoon/AdjectiveVerb

mmoon:AdjectiveVerb rdf:type owl:Class ;
                    
                    rdfs:subClassOf mmoon:Conversion ;
                    
                    mmoon:definition "An adjective verb is a verb which is fomed by conversion from an adjective and which entails no further derivational meaning except the entailed wordclass-change."@en ;
                    
                    mmoon:example "German: \"dicht\"(Adj.) 'impermeable' --> \"dichten\"(V) 'seal'."@de ;
                    
                    rdfs:label "Adjective verb"@en .



###  http://mmoon.org/mmoon/AdverbCompound

mmoon:AdverbCompound rdf:type owl:Class ;
                     
                     rdfs:subClassOf mmoon:Compounding ;
                     
                     mmoon:definition "An adverb compound is an adverb formed by composition of an adverb stem and another stem."@en ;
                     
                     mmoon:example "Eng.: \"downstream\"(Adv) = \"down\"(Adv) + \"stream\"(N)."@en ;
                     
                     rdfs:label "Adverb compound"@en .



###  http://mmoon.org/mmoon/Affix

mmoon:Affix rdf:type owl:Class ;
            
            owl:equivalentClass gold:Affix ;
            
            rdfs:subClassOf mmoon:Morph ;
            
            mmoon:definition "An affix is a bound segmental morph that is joined to a stem or root before, after, or within it, in the formation of a derived or inflected form. [Christian Lehmann, under \"affix\" at http://linguistik.uni-erfurt.de:8080/lido/Lido ]"@en ;
            
            rdfs:label "Affix"@en .



###  http://mmoon.org/mmoon/AgentNoun

mmoon:AgentNoun rdf:type owl:Class ;
                
                rdfs:subClassOf mmoon:DeverbalNoun ;
                
                mmoon:definition "A noun which is derived from a verb stem denoting someone or something that performs the action of the verb base."@en ;
                
                mmoon:example "Eng. \"to work\"(V) --> \"worker\"(N)."@en ;
                
                rdfs:label "Agent noun"@en .



###  http://mmoon.org/mmoon/AnalyticWordform

mmoon:AnalyticWordform rdf:type owl:Class ;
                       
                       rdfs:subClassOf mmoon:Wordform ;
                       
                       mmoon:definition """A word form is analytic iff it consists of more than one word form such that the lexical meaning provides the root of one of them, while the grammatical meaning components are coded in the other word forms (some of them possibly in the lexical word form).
An inflection is analytic to the extent that it consists of analytic word forms.[Christian Lehmann, under \"analytic structure\" at http://linguistik.uni-erfurt.de:8080/lido/Lido]"""@en ;
                       
                       mmoon:example """The comparison of adjectives in English involves both inflectional forms (e.g. bigger) and periphrastic constructions (e.g. more interesting).
Engl. sang (synthetic) vs. has sung (periphrastic). [Christian Lehmann, under \"periphrasis\" at http://linguistik.uni-erfurt.de:8080/lido/Lido]"""@en ;
                       
                       mmoon:externalDefinitionDesc """Delimitation and History:
’analytic’ is extensionally equivalent to ‚periphrastic’, but is also used to characterize a whole language type.
An analytic system has grammatical words; an isolating system does not.

Periphrastic conjugation is the formation of a verbal inflectional paradigm which consists of periphrastic forms.
Periphrastic conjugation is the formation of a verbal inflectional paradigm which consists of periphrastic forms.

Periphrasis is the combination of a grammatical word G with a lexical word L such that the result is equivalent to a morphological form of L inflected for the category of G.

Analytic Procedures:
Within one language, periphrasis is recognized by a paradigmatic relation between a periphrastic and a synthetic expression. Across languages or stages of one language, periphrasis in a looser sense is recognized on the basis of functional similarity. [Christian Lehmann, under \"analytic structure\" at http://linguistik.uni-erfurt.de:8080/lido/Lido]"""@en ;
                       
                       rdfs:label "Analytic word-form"@en .



###  http://mmoon.org/mmoon/ApplicativeVerb

mmoon:ApplicativeVerb rdf:type owl:Class ;
                      
                      rdfs:subClassOf mmoon:DeverbalVerb ;
                      
                      mmoon:definition "A verb which is derived from another intransitive verb stem resulting in a transitive verb."@en ;
                      
                      mmoon:example "German: \"steigen\"(V) 'rise' --> \"besteigen\"(V) 'climb'."@de ;
                      
                      mmoon:externalDefinitionDesc """Christian Lehmann, under \"applicative verb\" at http://linguistik.uni-erfurt.de:8080/lido/Lido :
An applicative verb is a derived transitive verb whose direct object has been promoted to this function from a more peripheral syntactic function that it has with the base verb.

The base may be transitive, in which case its direct object is demoted upon the derivation of the applicative verb."""@en ;
                      
                      rdfs:label "Applicative verb"@en .



###  http://mmoon.org/mmoon/AtomicMorpheme

mmoon:AtomicMorpheme rdf:type owl:Class ;
                     
                     rdfs:subClassOf mmoon:Morpheme ;
                     
                     mmoon:definition """A morpheme is atomic if the morph by which it is realized encodes exactly one meaning. Hence, an atomic morpheme has one mmon:Meaning. 

An exception is made for person and number morphemes, e.g. the third person singular \"-s\" suffix in English corresponds to the atomic morpheme \"1SG\"."""@en ;
                     
                     mmoon:example "The English \"-s\" plural morpheme."@en ;
                     
                     rdfs:label "Atomic morpheme"@en .



###  http://mmoon.org/mmoon/AugmentativeNoun

mmoon:AugmentativeNoun rdf:type owl:Class ;
                       
                       rdfs:subClassOf mmoon:DenominalNoun ;
                       
                       mmoon:definition "A noun which is derived from another noun stem that denotes big or large size. Possible connotations include strong, imposing, ugly."@en ;
                       
                       mmoon:externalDefinition gold:AugmentativeSize ,
                                                <http://purl.org/olia/olia.owl#Augmentative> ;
                       
                       mmoon:externalDefinitionDesc """Christian Lehmann, under \"augmentation\" at http://linguistik.uni-erfurt.de:8080/lido/Lido :
Augmentation is the derivational modification of a noun by the concept „big, large“. Possible connotations include „strong, imposing, ugly“.
A noun is augmentative 1 iff it is derived by augmentation."""@en ;
                       
                       rdfs:label "Augmentative noun"@en .



###  http://mmoon.org/mmoon/CausativeVerb

mmoon:CausativeVerb rdf:type owl:Class ;
                    
                    rdfs:subClassOf mmoon:DeverbalVerb ;
                    
                    mmoon:definition "A verb which is derived from another verb stem denoting  the causal role of a referent in relation to an event or state expressed by the deriving verb stem."@en ;
                    
                    mmoon:example """Eng.: \"fall\"(V) --> \"fell\"(V).
[Christian Lehmann, under \"causative verb\" at http://linguistik.uni-erfurt.de:8080/lido/Lido]"""@en ;
                    
                    mmoon:externalDefinitionDesc """Christian Lehmann, under \"causative verb\" at http://linguistik.uni-erfurt.de:8080/lido/Lido : 
A causative verb is an n-place verb (with n > 1) which is derived from an n-1-place verb in such a way that the additional semantic slot is mapped onto the subject slot and is occupied by a superordinate agent which causes the situation designated by the base.

An underived verb cannot be causative by this definition. However, the paradigmatic lexical relation of Engl. «kill» to «die» may be called ‚semantically causative’."""@en ;
                    
                    rdfs:label "Causative verb"@en .



###  http://mmoon.org/mmoon/Circumfix

mmoon:Circumfix rdf:type owl:Class ;
                
                owl:equivalentClass gold:Circumfix ;
                
                rdfs:subClassOf mmoon:Affix ;
                
                mmoon:definition "A circumfix is an affix which consits of a prefix and a suffix. [Christian Lehmann, under \"circumfix\" at http://linguistik.uni-erfurt.de:8080/lido/Lido ]"@en ;
                
                rdfs:label "Circumfix"@en .



###  http://mmoon.org/mmoon/CollectivePersonNoun

mmoon:CollectivePersonNoun rdf:type owl:Class ;
                           
                           rdfs:subClassOf mmoon:DenominalNoun ;
                           
                           mmoon:definition "A noun which is derived from another noun stem that denotes group of persons."@en ;
                           
                           mmoon:example "German: \"Lehrer\"(N) 'teacher' --> \"Lehrerschaft\"(N) 'body of teachers'."@de ;
                           
                           rdfs:label "Collective person noun"@en .



###  http://mmoon.org/mmoon/ComplexMorpheme

mmoon:ComplexMorpheme rdf:type owl:Class ;
                      
                      rdfs:subClassOf mmoon:Morpheme ;
                      
                      mmoon:definition "A  morpheme is complex if it corresponds to more than one morph, and therefore consists of fusional and/or atomic and/or empty morphemes."@en ;
                      
                      mmoon:example "The English noun stem \"driver\" consists of the verbal stem \"drive\" and the derivational suffix \"-er\". The corresponding complex morpheme is \"drive.PRS-AG.NMLZ\", which consists of the two fusional morphemes \"drive.PRS\" and \"AG.NMLZ\"."@en ;
                      
                      mmoon:usageNote """The declaration of such a morpheme is somewhat contrary to the definition of morpheme as \"smallest meaning-bearing units of a language\". Since each mmoon:Morph resource should be able to correspond to some morpheme the construct of a complex morpheme is established. This is often the case for derived stems.
 In the URI of the complex morpheme the morphemes of which it consists are separated by a hyphen."""@en ;
                      
                      rdfs:label "Complex morpheme"@en .



###  http://mmoon.org/mmoon/CompoundWord

mmoon:CompoundWord rdf:type owl:Class ;
                   
                   owl:equivalentClass gold:Compound ;
                   
                   rdfs:subClassOf mmoon:Lexeme ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty mmoon:compoundingRelation ;
                                     owl:someValuesFrom mmoon:Compounding
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty mmoon:isComposedOf ;
                                     owl:qualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                     owl:onClass [ rdf:type owl:Class ;
                                                   owl:unionOf ( mmoon:GrammaticalWord
                                                                 mmoon:Lexeme
                                                               )
                                                 ]
                                   ] ;
                   
                   mmoon:definition """A compound is a stem formed by composition. [Christian Lehmann, under \"compound\" at http://linguistik.uni-erfurt.de:8080/lido/Lido]

Is a kind of lexeme which is formed by the combination of two stems."""@en ;
                   
                   mmoon:example "Engl. \"playground\"."@en ;
                   
                   mmoon:usageNote "The compound word is specified for the two stems of which it is composed by using the property :consistsOfMorph, thus explicating the morphs which are involved in the word-formation of the compound word."@en ;
                   
                   rdfs:label "Compound Word"@en .



###  http://mmoon.org/mmoon/Compounding

mmoon:Compounding rdf:type owl:Class ;
                  
                  rdfs:subClassOf mmoon:WordFormation ;
                  
                  mmoon:definition "Is the process of lexeme-formation by combining two stems."@en ;
                  
                  mmoon:externalDefinitionDesc """Christian Lehmann, under \"compounding\" at http://linguistik.uni-erfurt.de:8080/lido/Lido :
Compounding [compositio thematum] is the formation of a stem by combination of two stems. Both stems may already be complex."""@en ;
                  
                  mmoon:usageNote "Compounds, in English and some other languages, can be written as one word or two separate words, and they can be hyphenated as well. [http://www.glottopedia.org/index.php/Compound]"@en ;
                  
                  rdfs:label "Compounding"@en .



###  http://mmoon.org/mmoon/Conjugation

mmoon:Conjugation rdf:type owl:Class ;
                  
                  rdfs:subClassOf mmoon:VerbalInflection ;
                  
                  mmoon:definition "Is the inflection of a verb. Conjugation categories commonly include person, number, gender, aspect, tense, mood, voice and non-finite subcategories. [Christian Lehmann, under \"conjugation\" at http://linguistik.uni-erfurt.de:8080/lido/Lido ]"@en ;
                  
                  rdfs:label "Conjugation"@en .



###  http://mmoon.org/mmoon/Conversion

mmoon:Conversion rdf:type owl:Class ;
                 
                 rdfs:subClassOf mmoon:Derivation ;
                 
                 mmoon:definition "Conversion, also called zero-derivation, is the formation of a lexeme from another part of speech which entails no further derivational meaning except that which is entailed in the wordclass-change."@en ;
                 
                 mmoon:externalDefinition <http://www.glottopedia.org/index.php/Conversion> ;
                 
                 mmoon:externalDefinitionDesc """Christian Lehmann, under \"conversion\" at http://linguistik.uni-erfurt.de:8080/lido/Lido :
Conversion [recategorizatio] is the implicit change of the word class of a stem.
Conversion [recategorizatio] is not a kind of derivation 1 if the latter is conceived as involving a morphological process. At the level of formal representation, however, conversion [recategorizatio] may be conceived as dependency of a stem on a zero affix."""@en ;
                 
                 mmoon:usageNote "All other derivations which include an affix marking the wordclass-change but also do not entail further derivational meaning belong into the according mmoon:Derived[Wordclass] subclasses."@en ;
                 
                 rdfs:label "Conversion"@en .



###  http://mmoon.org/mmoon/DeadjectivalAdjective

mmoon:DeadjectivalAdjective rdf:type owl:Class ;
                            
                            rdfs:subClassOf mmoon:DerivedAdjective ;
                            
                            mmoon:definition "Is an adjective lexeme which derived from another adjective stem."@en ;
                            
                            rdfs:label "Deadjectival adjective"@en .



###  http://mmoon.org/mmoon/DeadjectivalAdverb

mmoon:DeadjectivalAdverb rdf:type owl:Class ;
                         
                         rdfs:subClassOf mmoon:DerivedAdverb ;
                         
                         mmoon:definition "Is an adverb lexeme which derived from an adjective stem."@en ;
                         
                         mmoon:usageNote "Add instance here, if the language of the given morpheme inventory exhibits affixal marking for word-class change but adds NO further derivational meaning(s). For all derived lexemes that consist of a derivational affix denoting some additional meaning, the respective subclass must be instantiated or newly created."@en ;
                         
                         rdfs:label "DeadjectivalAdverb"@en .



###  http://mmoon.org/mmoon/DeadjectivalNoun

mmoon:DeadjectivalNoun rdf:type owl:Class ;
                       
                       rdfs:subClassOf mmoon:DerivedNoun ;
                       
                       mmoon:definition "Is a noun lexeme which derived from an adjective stem."@en ;
                       
                       mmoon:usageNote "Add instance here, if the language of the given morpheme inventory exhibits affixal marking for word-class change but adds NO further derivational meaning(s). For all derived lexemes that consist of a derivational affix denoting some additional meaning, the respective subclass must be instantiated or newly created."@en ;
                       
                       rdfs:label "Deadjectival noun"@en .



###  http://mmoon.org/mmoon/DeadjectivalVerb

mmoon:DeadjectivalVerb rdf:type owl:Class ;
                       
                       rdfs:subClassOf mmoon:DerivedVerb ;
                       
                       mmoon:definition "Is a verb lexeme which derived from an adjective stem."@en ;
                       
                       mmoon:usageNote "Add instance here, if the language of the given morpheme inventory exhibits affixal marking for word-class change but adds NO further derivational meaning(s). For all derived lexemes that consist of a derivational affix denoting some additional meaning, the respective subclass must be instantiated or newly created."@en ;
                       
                       rdfs:label "Deadjectival verb"@en .



###  http://mmoon.org/mmoon/DeadverbalAdverb

mmoon:DeadverbalAdverb rdf:type owl:Class ;
                       
                       rdfs:subClassOf mmoon:DerivedAdverb ;
                       
                       mmoon:definition "Is an adverb lexeme which derived from another adverb stem."@en ;
                       
                       rdfs:label "Deadverbal adverb"@en .



###  http://mmoon.org/mmoon/Declension

mmoon:Declension rdf:type owl:Class ;
                 
                 rdfs:subClassOf mmoon:NominalInflection ;
                 
                 mmoon:definition "Is the inflection of a noun. Relevant categories commonly include gender, number, case, possessive and definiteness. [Christian Lehmann, under \"declension\" at http://linguistik.uni-erfurt.de:8080/lido/Lido ]"@en ;
                 
                 rdfs:label "Declension"@en .



###  http://mmoon.org/mmoon/Degree

mmoon:Degree rdf:type owl:Class ;
             
             rdfs:subClassOf mmoon:AdjectivalInflection ;
             
             mmoon:definition "Attributively (but also predicatively) used adjectives inflect for the category degree of comparison. In agreement with the noun it modifies it can also be marked for nominal inflectional categories such as gender, case and number."@en ;
             
             rdfs:label "Degree"@en .



###  http://mmoon.org/mmoon/DenominalAdjective

mmoon:DenominalAdjective rdf:type owl:Class ;
                         
                         rdfs:subClassOf mmoon:DerivedAdjective ;
                         
                         mmoon:definition "Is an adjective lexeme which derived from noun stem."@en ;
                         
                         mmoon:usageNote "Add instance here, if the language of the given morpheme inventory exhibits affixal marking for word-class change but adds NO further derivational meaning(s). For all derived lexemes that consist of a derivational affix denoting some additional meaning, the respective subclass must be instantiated or newly created."@en ;
                         
                         rdfs:label "Denominal adjective"@en .



###  http://mmoon.org/mmoon/DenominalAdverb

mmoon:DenominalAdverb rdf:type owl:Class ;
                      
                      rdfs:subClassOf mmoon:DerivedAdverb ;
                      
                      mmoon:definition "Is an adverb lexeme which derived from a noun stem."@en ;
                      
                      mmoon:usageNote "Add instance here, if the language of the given morpheme inventory exhibits affixal marking for word-class change but adds NO further derivational meaning(s). For all derived lexemes that consist of a derivational affix denoting some additional meaning, the respective subclass must be instantiated or newly created."@en ;
                      
                      rdfs:label "Denominal adverb"@en .



###  http://mmoon.org/mmoon/DenominalNoun

mmoon:DenominalNoun rdf:type owl:Class ;
                    
                    rdfs:subClassOf mmoon:DerivedNoun ;
                    
                    mmoon:definition "Is a noun lexeme which derived from another noun stem."@en ;
                    
                    rdfs:label "Denominal noun"@en .



###  http://mmoon.org/mmoon/DenominalVerb

mmoon:DenominalVerb rdf:type owl:Class ;
                    
                    rdfs:subClassOf mmoon:DerivedVerb ;
                    
                    mmoon:definition "Is a verb lexeme which derived from a noun stem."@en ;
                    
                    mmoon:usageNote "Add instance here, if the language of the given morpheme inventory exhibits affixal marking for word-class change but adds NO further derivational meaning(s). For all derived lexemes that consist of a derivational affix denoting some additional meaning, the respective subclass must be instantiated or newly created."@en ;
                    
                    rdfs:label "Denominal verb"@en .



###  http://mmoon.org/mmoon/Derivation

mmoon:Derivation rdf:type owl:Class ;
                 
                 rdfs:subClassOf mmoon:WordFormation ;
                 
                 mmoon:definition "Is the process of lexeme-formation which is morphologically conditioned and involves the modification of a stem."@en ;
                 
                 rdfs:label "Derivation"@en .



###  http://mmoon.org/mmoon/DerivedAdjective

mmoon:DerivedAdjective rdf:type owl:Class ;
                       
                       rdfs:subClassOf mmoon:Derivation ;
                       
                       mmoon:definition "Is an adjective lexeme which derived from another stem."@en ;
                       
                       rdfs:label "Derived adjective"@en .



###  http://mmoon.org/mmoon/DerivedAdverb

mmoon:DerivedAdverb rdf:type owl:Class ;
                    
                    rdfs:subClassOf mmoon:Derivation ;
                    
                    mmoon:definition "Is an adverb lexeme which derived from another stem."@en ;
                    
                    rdfs:label "Derived adverb"@en .



###  http://mmoon.org/mmoon/DerivedNoun

mmoon:DerivedNoun rdf:type owl:Class ;
                  
                  rdfs:subClassOf mmoon:Derivation ;
                  
                  mmoon:definition "Is a noun lexeme which derived from another stem."@en ;
                  
                  rdfs:label "Derived noun"@en .



###  http://mmoon.org/mmoon/DerivedVerb

mmoon:DerivedVerb rdf:type owl:Class ;
                  
                  rdfs:subClassOf mmoon:Derivation ;
                  
                  mmoon:definition "Is a verb lexeme which derived from another stem."@en ;
                  
                  rdfs:label "Derived verb"@en .



###  http://mmoon.org/mmoon/DerivedWord

mmoon:DerivedWord rdf:type owl:Class ;
                  
                  rdfs:subClassOf mmoon:Lexeme ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty mmoon:derivationalRelation ;
                                    owl:someValuesFrom mmoon:Derivation
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty mmoon:isDerivedFrom ;
                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onClass [ rdf:type owl:Class ;
                                                  owl:unionOf ( mmoon:GrammaticalWord
                                                                mmoon:Lexeme
                                                              )
                                                ]
                                  ] ;
                  
                  mmoon:definition """A derivate is a stem generated by derivation [derivatio]. [Christian Lehmann, under \"derivate\" at http://linguistik.uni-erfurt.de:8080/lido/Lido]

Is a kind of lexeme which is formed by the morphological modification of another lexical stem."""@en ;
                  
                  mmoon:example "\"replay\" is a derived word from the lexeme \"play\"."@en ;
                  
                  mmoon:usageNote "The derived word is specified for the stem from which it is derived as well as for the affix that is involved in the derivation by using the property :consistsOfMorph, thus explicating the morphs which are involved in the word-formation of the derived word."@en ;
                  
                  rdfs:label "Derived word"@en .



###  http://mmoon.org/mmoon/DesiderativeVerb

mmoon:DesiderativeVerb rdf:type owl:Class ;
                       
                       rdfs:subClassOf mmoon:DeverbalVerb ;
                       
                       mmoon:definition "A verb which is derived from another verb stem denoting to want the meaning of the deriving verb stem."@en ;
                       
                       mmoon:example "Sanskrit: \"‚s:r.no-ti\" (V) 'hears' --> \"‚su-’sr¡u-.sa-ti\"(V) (RED-hear-DES-3.SG) 'wants to hear'."@sa ;
                       
                       rdfs:label "Desiderative verb"@en .



###  http://mmoon.org/mmoon/DeverbalAdjective

mmoon:DeverbalAdjective rdf:type owl:Class ;
                        
                        rdfs:subClassOf mmoon:DerivedAdjective ;
                        
                        mmoon:definition "Is an adjective lexeme which derived from a verb stem."@en ;
                        
                        mmoon:usageNote "Add instance here, if the language of the given morpheme inventory exhibits affixal marking for word-class change but adds NO further derivational meaning(s). For all derived lexemes that consist of a derivational affix denoting some additional meaning, the respective subclass must be instantiated or newly created."@en ;
                        
                        rdfs:label "Deverbal Adjective"@en .



###  http://mmoon.org/mmoon/DeverbalAdverb

mmoon:DeverbalAdverb rdf:type owl:Class ;
                     
                     rdfs:subClassOf mmoon:DerivedAdverb ;
                     
                     mmoon:definition "Is an adverb lexeme which derived from a verb stem."@en ;
                     
                     mmoon:usageNote "Add instance here, if the language of the given morpheme inventory exhibits affixal marking for word-class change but adds NO further derivational meaning(s). For all derived lexemes that consist of a derivational affix denoting some additional meaning, the respective subclass must be instantiated or newly created."@en ;
                     
                     rdfs:label "Deverbal adverb"@en .



###  http://mmoon.org/mmoon/DeverbalNoun

mmoon:DeverbalNoun rdf:type owl:Class ;
                   
                   rdfs:subClassOf mmoon:DerivedNoun ;
                   
                   mmoon:definition "Is a noun lexeme which derived from a verb stem."@en ;
                   
                   mmoon:usageNote "Add instance here, if the language of the given morpheme inventory exhibits affixal marking for word-class change but adds NO further derivational meaning(s). For all derived lexemes that consist of a derivational affix denoting some additional meaning, the respective subclass must be instantiated or newly created."@en ;
                   
                   rdfs:label "Deverbal noun"@en .



###  http://mmoon.org/mmoon/DeverbalVerb

mmoon:DeverbalVerb rdf:type owl:Class ;
                   
                   rdfs:subClassOf mmoon:DerivedVerb ;
                   
                   mmoon:definition "Is a verb lexeme which derived from another verb stem."@en ;
                   
                   rdfs:label "Deverbal verb"@en .



###  http://mmoon.org/mmoon/DiminutiveAdjective

mmoon:DiminutiveAdjective rdf:type owl:Class ;
                          
                          rdfs:subClassOf mmoon:DeadjectivalAdjective ;
                          
                          mmoon:definition "An adjective which is derived from another adjective stem that denotes a smaller intensity than the deriving adjective stem."@en ;
                          
                          mmoon:example "Eng. \"small\"(ADJ) --> \"smallish\"(ADJ)"@en ;
                          
                          mmoon:externalDefinition gold:DiminuativeSize ;
                          
                          rdfs:label "Diminutive adjective"@en .



###  http://mmoon.org/mmoon/DiminutiveNoun

mmoon:DiminutiveNoun rdf:type owl:Class ;
                     
                     rdfs:subClassOf mmoon:DenominalNoun ;
                     
                     mmoon:definition "A noun which is derived from another noun stem that denotes endearment, small size or small intensity."@en ;
                     
                     mmoon:example "Eng. \"duck\"(N) --> \"duckling\"(N)"@en ,
                                   "German \"Kind\"(N) --> \"Kindchen\"(N)"@de ;
                     
                     mmoon:externalDefinition gold:DiminuativeSize ,
                                              <http://purl.org/olia/olia.owl#DiminutiveNoun> ;
                     
                     rdfs:label "Diminutive noun"@en .



###  http://mmoon.org/mmoon/EmptyMorph

mmoon:EmptyMorph rdf:type owl:Class ;
                 
                 rdfs:subClassOf mmoon:Affix ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty mmoon:correspondsToMorpheme ;
                                   owl:allValuesFrom mmoon:EmptyMorpheme
                                 ] ;
                 
                 mmoon:definition "A morph that has no meaning but that must be posited for the sake of descriptive elegance. [Haspelmath and Sims: Understanding morphology. 2002:327]"@en ;
                 
                 rdfs:label "Empty morph"@en .



###  http://mmoon.org/mmoon/EmptyMorpheme

mmoon:EmptyMorpheme rdf:type owl:Class ;
                    
                    rdfs:subClassOf mmoon:Morpheme ;
                    
                    mmoon:definition "An empty morpheme serves as means to state that a morph (except for the zero morph) corresponds to no meaning."@en ;
                    
                    mmoon:usageNote "An empty morpheme is realized by an empty morph. Often an empty morph is realized for the sake of euphony."@en ;
                    
                    rdfs:label "Empty morpheme"@en .



###  http://mmoon.org/mmoon/FactitiveVerb

mmoon:FactitiveVerb rdf:type owl:Class ;
                    
                    rdfs:subClassOf mmoon:DeadjectivalVerb ;
                    
                    mmoon:definition "A verb which is derived from an adjective stem denoting causation."@en ;
                    
                    mmoon:example """Engl.:\"black\"(Adj) --> \"blacken\"(Vtr.).
 [Christian Lehmann, under \"factitive verb\" at http://linguistik.uni-erfurt.de:8080/lido/Lido]"""@en ,
                                  """German: \"tot\"(Adj) 'dead' --> \"töten\"(V) 'to kill'.
 [Christian Lehmann, under \"factitive verb\" at http://linguistik.uni-erfurt.de:8080/lido/Lido]"""@de ;
                    
                    mmoon:externalDefinitionDesc """Christian Lehmann, under \"factitive verb\" at http://linguistik.uni-erfurt.de:8080/lido/Lido :
A factitive verb is a causative verb based on an adjective (or a stative intransitive verb).

Delimitation and History:
While there may be a subordinate agent in a causative construction, there may be none in a factitive one. This has sometimes been a reason for not subsuming factitive under causative."""@en ;
                    
                    rdfs:label "Factitive verb"@en .



###  http://mmoon.org/mmoon/FemaleNoun

mmoon:FemaleNoun rdf:type owl:Class ;
                 
                 rdfs:subClassOf mmoon:DenominalNoun ;
                 
                 mmoon:definition "A noun which is derived from another noun stem and that denotes a female person."@en ;
                 
                 mmoon:example "German: \"Fahrer\"(N) 'male driver' --> \"Fahrerin\"(N) 'female driver'."@de ;
                 
                 rdfs:label "Female noun"@en .



###  http://mmoon.org/mmoon/FusionalMorpheme

mmoon:FusionalMorpheme rdf:type owl:Class ;
                       
                       rdfs:subClassOf mmoon:Morpheme ;
                       
                       mmoon:definition """A morpheme is fusional if it entails more than one meaning and if these meanings are expressed in one un-analyzable morph. Hence, a fusional morpheme has at least two mmon:Meaning(s). Each meaning is separated by a period in the URI of a fusional morpheme instance.
This often the case for inherent meanings such as number for nouns or tense for verbs."""@en ;
                       
                       mmoon:example "The English stem \"go\" of the verb \"to go\"  is a morph encoding the meaning of the root \"go\" and inherently also present tense. It corresponds to the fusional morpheme \"go.PRS\""@en ;
                       
                       rdfs:label "Fusional morpheme"@en .



###  http://mmoon.org/mmoon/GrammaticalWord

mmoon:GrammaticalWord rdf:type owl:Class ;
                      
                      rdfs:subClassOf mmoon:Word ;
                      
                      mmoon:definition "A grammatical word is a word which is a grammatical formative. [Christian Lehmann, under \"grammatical word\" at http://linguistik.uni-erfurt.de:8080/lido/Lido]"@en ;
                      
                      mmoon:example "Engl. «be», «of».  [Christian Lehmann, under \"grammatical word\" at http://linguistik.uni-erfurt.de:8080/lido/Lido]"@en ;
                      
                      rdfs:label "Grammatical word"@en .



###  http://mmoon.org/mmoon/Infix

mmoon:Infix rdf:type owl:Class ;
            
            owl:equivalentClass gold:Infix ;
            
            rdfs:subClassOf mmoon:Affix ;
            
            mmoon:definition "An infix is an affix that is added within an otherwise unanalysable root and disrupts this. [Christian Lehmann, under \"infix\" at http://linguistik.uni-erfurt.de:8080/lido/Lido ]"@en ;
            
            rdfs:label "Infix"@en .



###  http://mmoon.org/mmoon/Inflection

mmoon:Inflection rdf:type owl:Class ;
                 
                 rdfs:subClassOf mmoon:MorphologicalRelationship ;
                 
                 mmoon:definition "Is the process of generating word-forms of lexical stems. This class can be extended to describe the inflectional classes of the lexemes of a language."@en ;
                 
                 mmoon:externalDefinitionDesc """Christian Lehmann, under \"inflection\" at http://linguistik.uni-erfurt.de:8080/lido/Lido] :
Inflection [flexio] is the syntactically relevant part of morphology. It generates word forms (inflectional forms) of word stems.
In contrast to derivation, inflection  never results in a change of word class and produces a predictable, regular change of meaning (for regular word-forms only).
Its expression is typically located farther from the root than that of derivation."""@en ;
                 
                 mmoon:usageNote "In the subclasses of \"Inflection\" the semantic or grammatic categories are not extensively specified. They only serve to declare what kind of morphological relationship(s) is/are expressed on a word-form."@en ;
                 
                 rdfs:label "Inflection"@en .



###  http://mmoon.org/mmoon/InstrumentNoun

mmoon:InstrumentNoun rdf:type owl:Class ;
                     
                     rdfs:subClassOf mmoon:DeverbalNoun ;
                     
                     mmoon:definition "A noun which is derived from a verb stem denoting that some thing/instrument is used to carry out the meaning of the verb stem."@en ;
                     
                     mmoon:example "German: \"lenken\"(V) 'to steer'--> \"Lenker\"(N) 'handle-bar, steering wheel'. [Christian Lehmann, under \"instrument noun\" at http://linguistik.uni-erfurt.de:8080/lido/Lido]"@de ;
                     
                     mmoon:externalDefinitionDesc """Christian Lehmann, under \"instrument noun\" at http://linguistik.uni-erfurt.de:8080/lido/Lido :
An instrument noun is an oriented deverbal noun that designates something that functions as an instrument in the situation designated by the base verb. [Christian Lehmann, under \"instrument noun\" at http://linguistik.uni-erfurt.de:8080/lido/Lido]"""@en ;
                     
                     rdfs:label "Instrument noun"@en .



###  http://mmoon.org/mmoon/Introflection

mmoon:Introflection rdf:type owl:Class ;
                    
                    rdfs:subClassOf mmoon:Inflection ,
                                    mmoon:WordFormation ;
                    
                    mmoon:definition "Nonconcatenative morphology, also called discontinuous morphology and introflection, is a form of word-formation [and inflection] in which the root is modified and does not involve stringing morphemes together. [Haspelmath and Sims: Understanding morphology. 2002, Sections 3.1.2 - 3.1.4]"@en ;
                    
                    rdfs:label "Introflection"@en .



###  http://mmoon.org/mmoon/IrregularConjugation

mmoon:IrregularConjugation rdf:type owl:Class ;
                           
                           rdfs:subClassOf mmoon:Conjugation ;
                           
                           mmoon:definition "Conjugation that deviates from the regular inflectional pattern."@en ;
                           
                           rdfs:label "Irregular conjugation"@en .



###  http://mmoon.org/mmoon/IrregularDegree

mmoon:IrregularDegree rdf:type owl:Class ;
                      
                      rdfs:subClassOf mmoon:Degree ;
                      
                      mmoon:definition "Formation of degree of comparison that deviates from the regular inflectional pattern."@en ;
                      
                      rdfs:label "Irregular degree"@en .



###  http://mmoon.org/mmoon/Lexeme

mmoon:Lexeme rdf:type owl:Class ;
             
             owl:equivalentClass gold:LexicalItem ;
             
             rdfs:subClassOf mmoon:Word ;
             
             mmoon:definition "A lexeme is a word as it appears as an entry in a dictionary. It has a holistic abstract sense representing the core meaning shared by a set of closely-related word-forms. A lexeme can only be a word that inflects."@en ;
             
             mmoon:externalDefinitionDesc """Christian Lehmann, under \"lexeme\" at http://linguistik.uni-erfurt.de:8080/lido/Lido :
A lexeme [lexema] is a word in its capacity as a unit of the lexicon. In a text, it appears in form of diverse word forms."""@en ;
             
             rdfs:label "Lexeme"@en .



###  http://mmoon.org/mmoon/MaterialAdjective

mmoon:MaterialAdjective rdf:type owl:Class ;
                        
                        rdfs:subClassOf mmoon:DenominalAdjective ;
                        
                        mmoon:definition "An adjective which is derived from a noun stem denoting some kind of material and means 'being made of this material'."@en ;
                        
                        mmoon:example "German: Holz 'wood' --> hölzern 'wooden'."@de ;
                        
                        rdfs:label "Material adjective"@en .



###  http://mmoon.org/mmoon/Meaning

mmoon:Meaning rdf:type owl:Class ;
              
              mmoon:definition "This class comprises a wide range of meanings a linguistic sign can be associated with, e.g. linguistic categories, morphosyntactic features, lexical senses, derivational meanings."@en ;
              
              rdfs:label "Meaning"@en .



###  http://mmoon.org/mmoon/Morph

mmoon:Morph rdf:type owl:Class ;
            
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty mmoon:hasRepresentation ;
                              owl:someValuesFrom mmoon:Representation
                            ] ;
            
            mmoon:definition "A morph is a concrete realization of a single morpheme which usually results from segmentation."@en ;
            
            mmoon:usageNote "A morph resource describes only the perceptible side of a morpheme, i.e. the significans. As such it is not directly associated with a meaning but with a corresponding morpheme resource."@en ;
            
            rdfs:label "Morph"@en .



###  http://mmoon.org/mmoon/Morpheme

mmoon:Morpheme rdf:type owl:Class ;
               
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                 owl:onProperty mmoon:hasMeaning ;
                                 owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onClass mmoon:Meaning
                               ] ;
               
               mmoon:definition """The morpheme class usually contains the smallest meaning-bearing units of a language. These comprise all semantically or grammatically distinct parts which are identifiable by segmentation of  the morphs of which a word or a word-form consists.

Depending on the kind of correspondence betweene a morpheme its morph realization, four kinds of morphemes are established: 1) atomic morphemes, 2) fusional morphemes, 3) Empty morphemes and 4) complex morphemes."""@en ;
               
               mmoon:usageNote """A morpheme resource describes only the conceptual side, i.e. the significatum, which is realized by one or more morphs in a certain language. Thus, the meaning(s) it conveys must be always stated via the mmoon:hasMeaning property.

For convenience the morpheme instances are rendered by the same notatinal means as the morphemic glosses, i.e. the plural morpheme is rendered in short as \"PL\"."""@en ;
               
               rdfs:label "Morpheme"@en .



###  http://mmoon.org/mmoon/MorphemeInventory

mmoon:MorphemeInventory rdf:type owl:Class ;
                        
                        mmoon:definition "The morpheme inventory is the object that contains morphemic entries. It is specified for the natural language it describes."@en .



###  http://mmoon.org/mmoon/MorphemicGloss

mmoon:MorphemicGloss rdf:type owl:Class ;
                     
                     mmoon:definition "The gloss is the abstract identity of a morpheme. It serves as a metalinguistic representation of morphological meanings."@en ;
                     
                     mmoon:usageNote "Most of the glosses used for grammatical concepts are taken from the Leipzig Glossing Rules  (https://www.eva.mpg.de/lingua/pdf/LGR08.02.05.pdf) or from Lehmann 2004 (http://christianlehmann.eu/publ/img.pdf). Lexical senses are usually glossed in English by using the orthographic representation of the lexical element (such as stems and roots) but can be also given in the language of the MMoOn language inventory."@en ;
                     
                     rdfs:label "Morphemic gloss"@en .



###  http://mmoon.org/mmoon/MorphologicalRelationship

mmoon:MorphologicalRelationship rdf:type owl:Class ;
                                
                                mmoon:definition "Is the relationship between word-forms of a lexeme (inflection) or the relationship between lexemes of a word family (derivation and compounding). [Haspelmath and Sims: Understanding morphology. 2002:18]"@en ;
                                
                                rdfs:label "Morphological Relationship"@en .



###  http://mmoon.org/mmoon/NoDegree

mmoon:NoDegree rdf:type owl:Class ;
               
               rdfs:subClassOf mmoon:AdjectivalInflection ;
               
               mmoon:definition "Some adjectives do not inflect for the category degree of comparison, e.g. \"dead\", \"absolute\", \"pregnant\". In agreement with the noun it modifies it can still be marked for nominal inflectional categories such as gender, case and number."@en ;
               
               rdfs:label "No degree"@en .



###  http://mmoon.org/mmoon/NoInflection

mmoon:NoInflection rdf:type owl:Class ;
                   
                   rdfs:subClassOf mmoon:Inflection ;
                   
                   mmoon:definition "This class marks words which do not inflect. This holds for most grammatical words but also for some lexemes (e.g. the invariable adjective \"super\" in German)."@en ;
                   
                   rdfs:label "No inflection"@en .



###  http://mmoon.org/mmoon/NoWordFormation

mmoon:NoWordFormation rdf:type owl:Class ;
                      
                      rdfs:subClassOf mmoon:WordFormation ;
                      
                      mmoon:definition "Simple lexemes that cannot be further segmented into derivational or compositional components exhibit no kind of word-formation. Rather, they serve as the derivational or compositional basis for derived words and compound words."@en ;
                      
                      rdfs:label "No word-formation"@en .



###  http://mmoon.org/mmoon/NominalAdverb

mmoon:NominalAdverb rdf:type owl:Class ;
                    
                    rdfs:subClassOf mmoon:Conversion ;
                    
                    mmoon:definition "A nominal adverb is an adverb which is fomed by conversion from a noun and which entails no further derivational meaning except the entailed wordclass-change."@en ;
                    
                    mmoon:example "Engl.: \"backstage\"(N) --> \"backstage\"(Adv)."@en ;
                    
                    rdfs:label "Nominal adverb"@en .



###  http://mmoon.org/mmoon/NominalCompound

mmoon:NominalCompound rdf:type owl:Class ;
                      
                      rdfs:subClassOf mmoon:Compounding ;
                      
                      mmoon:definition "A nominal compound is a noun formed by composition of a nominal stem and another stem."@en ;
                      
                      mmoon:example "Engl.: \"blackboard\"(N) = \"black\"(Adj) + \"board\"(N)."@en ;
                      
                      rdfs:label "Nominal compound"@en .



###  http://mmoon.org/mmoon/NominalInflection

mmoon:NominalInflection rdf:type owl:Class ;
                        
                        rdfs:subClassOf mmoon:Inflection ;
                        
                        mmoon:definition "Is the process of generating word-forms of nominal stems."@en ;
                        
                        rdfs:label "Nominal inflection"@en .



###  http://mmoon.org/mmoon/NominalVerb

mmoon:NominalVerb rdf:type owl:Class ;
                  
                  rdfs:subClassOf mmoon:Conversion ;
                  
                  mmoon:definition "A nominal verb is a verb which is fomed by conversion from a noun and which entails no further derivational meaning except the entailed wordclass-change."@en ;
                  
                  mmoon:example "Engl.: \"the steam\"(N) --> \"to steam\"(V)."@en ;
                  
                  rdfs:label "Nominal verb"@en .



###  http://mmoon.org/mmoon/NounAdjective

mmoon:NounAdjective rdf:type owl:Class ;
                    
                    rdfs:subClassOf mmoon:Conversion ;
                    
                    mmoon:definition "A noun adjective is an adjective which is fomed by conversion from a noun and which entails no further derivational meaning except the entailed wordclass-change."@en ;
                    
                    mmoon:example "Engl.: \"Swiss\"(NPr) --> \"Swiss\"(Adj)."@en ;
                    
                    rdfs:label "Noun adjective"@en .



###  http://mmoon.org/mmoon/PredicativeAdj

mmoon:PredicativeAdj rdf:type owl:Class ;
                     
                     rdfs:subClassOf mmoon:AdjectivalInflection ;
                     
                     mmoon:definition "Predicatively used adjectives (in sentences such as \"Peter is small.\") charactarize the noun when occuring with copula verbs like \"be\" and do not inflect for nominal agreement. Relational adjectives (i.e. gradable adjectives) do still inflect for degree."@en ;
                     
                     rdfs:label "Predicative adjective"@en .



###  http://mmoon.org/mmoon/Prefix

mmoon:Prefix rdf:type owl:Class ;
             
             owl:equivalentClass gold:Prefix ;
             
             rdfs:subClassOf mmoon:Affix ;
             
             mmoon:definition "A prefix is an affix that precedes its stem or root. [Christian Lehmann, under \"prefix\" at http://linguistik.uni-erfurt.de:8080/lido/Lido ]"@en ;
             
             rdfs:label "Prefix"@en .



###  http://mmoon.org/mmoon/RegularConjugation

mmoon:RegularConjugation rdf:type owl:Class ;
                         
                         rdfs:subClassOf mmoon:Conjugation ;
                         
                         mmoon:definition "Conjugation that exhibits a regular inflectional pattern."@en ;
                         
                         rdfs:label "Regular conjugation"@en .



###  http://mmoon.org/mmoon/RegularDegree

mmoon:RegularDegree rdf:type owl:Class ;
                    
                    rdfs:subClassOf mmoon:Degree ;
                    
                    mmoon:definition "Formation of degree of comparison that exhibits a regular inflectional pattern."@en ;
                    
                    rdfs:label "Regular degree"@en .



###  http://mmoon.org/mmoon/Representation

mmoon:Representation rdf:type owl:Class ;
                     
                     rdfs:subClassOf [ rdf:type owl:Class ;
                                       owl:unionOf ( [ rdf:type owl:Restriction ;
                                                       owl:onProperty mmoon:orthographicRepresentation ;
                                                       owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                       owl:onDataRange xsd:string
                                                     ]
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty mmoon:phoneticRepresentation ;
                                                       owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                       owl:onDataRange xsd:string
                                                     ]
                                                   )
                                     ] ;
                     
                     mmoon:definition "A linguistic representation of a word or morph."@en ;
                     
                     rdfs:label "Representation"@en .



###  http://mmoon.org/mmoon/Root

mmoon:Root rdf:type owl:Class ;
           
           owl:equivalentClass gold:Root ;
           
           rdfs:subClassOf mmoon:Morph ;
           
           mmoon:definition "A morph that constitutes the semantic nucleus of a stem. It cannot be further segmented and is often not specified for a part of speech. There are lexical roots, like Engl. «get», and grammatical roots, like Engl. «wh-». [Christian Lehmann, under \"root\" at http://linguistik.uni-erfurt.de:8080/lido/Lido ]"@en ;
           
           rdfs:label "Root"@en .



###  http://mmoon.org/mmoon/Simulfix

mmoon:Simulfix rdf:type owl:Class ;
               
               rdfs:subClassOf mmoon:Affix ;
               
               mmoon:definition "A simulfix is a change or replacement of vowels or consonants (usually vowels) which changes the meaning of a word, e.g. \"eat\" in past tense becomes \"ate \"tooth\" becomes \"teeth\" when plural. [http://www-01.sil.org/linguistics/GlossaryOflinguisticTerms/WhatIsASimulfix.htm]"@en ;
               
               mmoon:example "In ‚man - men’, the latter contains the replacive morph ‚a -> e’. [Christian Lehmann, under \"replacive morph\" at http://linguistik.uni-erfurt.de:8080/lido/Lido]"@en ;
               
               mmoon:externalDefinition <http://www-01.sil.org/linguistics/GlossaryOflinguisticTerms/WhatIsASimulfix.htm> ;
               
               mmoon:externalDefinitionDesc """Also known as replacive morph: 
A replacive morph is a descriptive construct representing an internal modification.
Delimitation and History:
The replacive morph is a construct of taxonomic structuralism.
[Christian Lehmann, under \"replacive morph\" at http://linguistik.uni-erfurt.de:8080/lido/Lido]"""@en ;
               
               rdfs:label "Simulfix"@en .



###  http://mmoon.org/mmoon/Stem

mmoon:Stem rdf:type owl:Class ;
           
           owl:equivalentClass gold:Stem ;
           
           rdfs:subClassOf mmoon:Morph ;
           
           mmoon:definition """In a word-form: The stem is the morph to which inflectional marking applies. It is the morphological representative of the lexeme in a given word-form and is specified for part of speech.

In a derived word: The stem is the morphological representative of the lexical basis from which it is derived. 

In a compound word: The stem is the morphological representative of one of the two stems the word is composed of."""@en ;
           
           rdfs:label "Stem"@en .



###  http://mmoon.org/mmoon/Suffix

mmoon:Suffix rdf:type owl:Class ;
             
             owl:equivalentClass gold:Suffix ;
             
             rdfs:subClassOf mmoon:Affix ;
             
             mmoon:definition "A suffix is an affix that follows its stem or root. [Christian Lehmann, under \"suffix\" at http://linguistik.uni-erfurt.de:8080/lido/Lido ]"@en ;
             
             rdfs:label "Suffix"@en .



###  http://mmoon.org/mmoon/SyntheticWordform

mmoon:SyntheticWordform rdf:type owl:Class ;
                        
                        rdfs:subClassOf mmoon:Wordform ;
                        
                        mmoon:definition "A word form is synthetic 1 iff it is not analytic [i.e. all its semantic and grammatical components are represented in one word form]. [Christian Lehmann, under \"synthesis\" at http://linguistik.uni-erfurt.de:8080/lido/Lido]"@en ;
                        
                        mmoon:example "«went» is a synthetic word-form of the lexeme «go»."@en ;
                        
                        rdfs:label "Synthetic word-form"@en .



###  http://mmoon.org/mmoon/Transfix

mmoon:Transfix rdf:type owl:Class ;
               
               rdfs:subClassOf mmoon:Affix ;
               
               mmoon:definition "A transfix is a discontinuous affix at least one of whose components is an infix.  [Christian Lehmann, under \"transfix\" at http://linguistik.uni-erfurt.de:8080/lido/Lido ]"@en ;
               
               rdfs:label "Transfix"@en .



###  http://mmoon.org/mmoon/VerbalAdjective

mmoon:VerbalAdjective rdf:type owl:Class ;
                      
                      rdfs:subClassOf mmoon:Conversion ;
                      
                      mmoon:definition "A verbal adjective is an adjective which is fomed by conversion from a verb and which entails no further derivational meaning except the entailed wordclass-change."@en ;
                      
                      mmoon:example "German: \"regierend\"(VParticiple) 'to reign' --> \"regierend\"(Adj) 'reigning'."@de ;
                      
                      rdfs:label "Verbal adjective"@en .



###  http://mmoon.org/mmoon/VerbalCompound

mmoon:VerbalCompound rdf:type owl:Class ;
                     
                     rdfs:subClassOf mmoon:Compounding ;
                     
                     mmoon:definition "A verbal compound is a verb formed by composition of a verbal stem and another stem."@en ;
                     
                     mmoon:example "Eng.: \"force feed\"(V) = \"force\"(N) + \"feed\"(V)."@en ;
                     
                     rdfs:label "Verbal compound"@en .



###  http://mmoon.org/mmoon/VerbalInflection

mmoon:VerbalInflection rdf:type owl:Class ;
                       
                       rdfs:subClassOf mmoon:Inflection ;
                       
                       mmoon:definition "Is the process of generating word-forms of verbal stems."@en ;
                       
                       rdfs:label "Verbal inflection"@en .



###  http://mmoon.org/mmoon/VerbalNoun

mmoon:VerbalNoun rdf:type owl:Class ;
                 
                 rdfs:subClassOf mmoon:Conversion ;
                 
                 mmoon:definition "A verbal noun is a noun which is fomed by conversion from a verb and which entails no further derivational meaning except the entailed wordclass-change."@en ;
                 
                 mmoon:example "Engl.: \"to call\"(V) --> \"the call\"(N)."@en ;
                 
                 rdfs:label "Verbal noun"@en .



###  http://mmoon.org/mmoon/Word

mmoon:Word rdf:type owl:Class ;
           
           rdfs:subClassOf [ rdf:type owl:Restriction ;
                             owl:onProperty mmoon:hasRepresentation ;
                             owl:someValuesFrom mmoon:Representation
                           ] ;
           
           mmoon:definition """A word is a unit which is a constituent at the phrase level and above. It is sometimes identifiable according to such criteria as a) being the minimal possible unit in a reply, b) having features such as a regular stress pattern, and phonological changes conditioned by or blocked at word boundaries, c) being the largest unit resistant to insertion of new constituents within its boundaries, or d)  being the smallest constituent that can be moved within a sentence without making the sentence ungrammatical.
A word is sometimes placed, in a hierarchy of grammatical constituents, above the morpheme level and below the phrase level. [http://www-01.sil.org/linguistics/GlossaryOflinguisticTerms/WhatIsAWord.htm]"""@en ;
           
           mmoon:externalDefinitionDesc """Christian Lehmann, under \"word\" at http://linguistik.uni-erfurt.de:8080/lido/Lido :
A word is a (grammatical) unit that enjoys a moderate degree of autonomy. I.e. while its parts tend to occupy fixed (morphological) positions, to be welded phonologically, to contract well-defined paradigmatic relations, all this is true to a lesser extent for the word itself and to an even lesser extent for the syntagm.
The cognitive counterpart of a word in general is a mental representation."""@en ;
           
           rdfs:label "Word"@en .



###  http://mmoon.org/mmoon/WordFormation

mmoon:WordFormation rdf:type owl:Class ;
                    
                    rdfs:subClassOf mmoon:MorphologicalRelationship ;
                    
                    mmoon:definition """Is the formation of words based on other words by the means of derivation and compounding. 

Word-formation is also called lexeme-formation and stem-formation ."""@en ;
                    
                    mmoon:externalDefinitionDesc """Christian Lehmann, under \"word formation\" at http://linguistik.uni-erfurt.de:8080/lido/Lido :
Stem formation is the lexical part of morphology.
Stem formation is the formation of stems, normally on the basis of at least one component stem. Each stem thus generated potentially constitutes a lexeme [lexema] of its own."""@en ;
                    
                    rdfs:label "Word-formation"@en .



###  http://mmoon.org/mmoon/Wordform

mmoon:Wordform rdf:type owl:Class ;
               
               rdfs:subClassOf mmoon:Word ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty mmoon:consistsOfMorph ;
                                 owl:someValuesFrom mmoon:Stem
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty mmoon:inflectionalRelation ;
                                 owl:someValuesFrom mmoon:Inflection
                               ] ;
               
               mmoon:definition """A word form is one of the morphological manifestations of a lexeme in text. For all parts of speech that inflect, the specification of inflectional categories for a lexeme generates the set of its word forms. [Christian Lehmann, under \"word form\" at http://linguistik.uni-erfurt.de:8080/lido/Lido]

It expresses the combination of a lexeme and a set of grammatical meanings. A word-form always is an inflectional variant of a lexeme."""@en ;
               
               mmoon:example "«said» is a word form of «say». [Christian Lehmann, under \"word form\" at http://linguistik.uni-erfurt.de:8080/lido/Lido]"@en ;
               
               mmoon:usageNote "The word-form is specified for the stem to which the inflectional or agreement markings apply and for the inflectional and agreement affixes which are involved by using the property :consistsOfMorph, thus explicating the morphs which are involved in the inflectional pattern of the word-form."@en ;
               
               rdfs:label "Word-form"@en .



###  http://mmoon.org/mmoon/ZeroMorph

mmoon:ZeroMorph rdf:type owl:Class ;
                
                rdfs:subClassOf mmoon:Affix ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty mmoon:isAllomorphTo ;
                                  owl:someValuesFrom mmoon:Morph
                                ] ;
                
                mmoon:definition "A morpheme that has a morphological meaning that corresponds to no overt form.  [Haspelmath and Sims: Understanding morphology. 2002:64]"@en ;
                
                mmoon:externalDefinitionDesc "A zero morpheme is a morpheme which has no significans. [Christian Lehmann, under \"zero morpheme\" at http://linguistik.uni-erfurt.de:8080/lido/Lido]"@en ;
                
                mmoon:usageNote "A zero morpheme is posited only if it is in equipollent opposition to overt morphemes. If the term in question is, instead, the unmarked member of a privative opposition, no morpheme is posited. Conversion  can be conceived as derivation using a zero morpheme.  [Christian Lehmann, under \"zero morpheme\" at http://linguistik.uni-erfurt.de:8080/lido/Lido]"@en ;
                
                rdfs:label "Zero morph"@en .



