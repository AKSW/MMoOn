mmoon:ACN rdf:type owl:NamedIndividual ,
                   mmoon:MorphemicGloss ;
          
          rdfs:comment "Action noun gloss."@en ;
          
          rdfs:label "ACN"^^xsd:string .



###  http://mmoon.org/mmoon/ACT

mmoon:ACT rdf:type owl:NamedIndividual ,
                   mmoon:MorphemicGloss ;
          
          rdfs:comment "Active gloss."@en ;
          
          rdfs:label "ACT"^^xsd:string .



###  http://mmoon.org/mmoon/ACTR

mmoon:ACTR rdf:type owl:NamedIndividual ,
                    mmoon:MorphemicGloss ;
           
           rdfs:comment "Actor gloss."@en ;
           
           rdfs:label "ACTR"^^xsd:string .



###  http://mmoon.org/mmoon/ADESS

mmoon:ADESS rdf:type owl:NamedIndividual ,
                     mmoon:MorphemicGloss ;
            
            rdfs:comment "Adessive gloss."@en ;
            
            rdfs:label "ADESS"^^xsd:string .



###  http://mmoon.org/mmoon/ADJ

mmoon:ADJ rdf:type owl:NamedIndividual ,
