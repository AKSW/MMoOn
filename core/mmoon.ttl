@prefix : <http://mmoon.org/mmoon/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix gold: <http://purl.org/linguistics/gold/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix lexvo: <http://lexvo.org/ontology#> .
@prefix mmoon: <http://mmoon.org/mmoon/> .
@base <http://mmoon.org/mmoon/> .

<http://mmoon.org/mmoon/> rdf:type owl:Ontology ;
                          
                          rdfs:label "Multilingual Morpheme Ontology"@en ;
                          
                          rdfs:comment """The Multilingual Morpheme Ontology (MMoOn) is  an ontological model that contains the vocabulary needed to describe the morphological elements, meanings and representations of natural languages. 

This is the reference ontology from which various language specific MMoOn Inventories can be derived."""@en ;
                          
                          owl:versionInfo "Version 1.0, 2016"@en .


#################################################################
#
#    Annotation properties
#
#################################################################


###  http://mmoon.org/mmoon/definition

:definition rdf:type owl:AnnotationProperty ;
            
            rdfs:comment "A definition for MMoOn vocabulary item (classes, properties, also possibly individuals)."@en ;
            
            rdfs:subPropertyOf rdfs:comment ,
                               skos:definition .



###  http://mmoon.org/mmoon/example

:example rdf:type owl:AnnotationProperty ;
         
         rdfs:subPropertyOf rdfs:comment .



###  http://mmoon.org/mmoon/externalDefinition

:externalDefinition rdf:type owl:AnnotationProperty ;
                    
                    rdfs:comment """A reference to an (typically alternative) definition for MMoOn vocabulary items. 

Solely to be used to refer to Web Resources identified by their URI/IRI (which ideally should be Linked Data)."""@en ;
                    
                    rdfs:range rdfs:Resource ;
                    
                    rdfs:subPropertyOf rdfs:comment .



###  http://mmoon.org/mmoon/externalDefinitionDesc

:externalDefinitionDesc rdf:type owl:AnnotationProperty ;
                        
                        rdfs:comment """A reference to an (typically alternative) definition for MMoOn vocabulary items. 

To be used when no URI/IRI can be provided as
reference point or when additional textual information has to be provided for an URI/IRI reference to be sufficiently specific."""@en ;
                        
                        rdfs:subPropertyOf rdfs:comment ;
                        
                        rdfs:range xsd:string .



###  http://mmoon.org/mmoon/usageNote

:usageNote rdf:type owl:AnnotationProperty ;
           
           rdfs:subPropertyOf rdfs:comment .



###  http://www.w3.org/2004/02/skos/core#definition

skos:definition rdf:type owl:AnnotationProperty .





#################################################################
#
#    Object Properties
#
#################################################################


###  http://mmoon.org/mmoon/associatedToLanguage

:associatedToLanguage rdf:type owl:ObjectProperty ;
                      
                      rdfs:label "associated to language"@en ;
                      
                      rdfs:comment "States that a morpheme inventory, morph or word is associated to a specific langauge."@en ;
                      
                      rdfs:range lexvo:Language .



###  http://mmoon.org/mmoon/attachedToRoot

:attachedToRoot rdf:type owl:ObjectProperty ;
                
                rdfs:label "attached to root"@en ;
                
                rdfs:comment "States that an affix is attached to some root."@en ;
                
                rdfs:domain :Affix ;
                
                rdfs:range :Root .



###  http://mmoon.org/mmoon/attachedToStem

:attachedToStem rdf:type owl:ObjectProperty ;
                
                rdfs:label "attached to stem"@en ;
                
                rdfs:comment "States that an affix is attached to some stem."@en ;
                
                rdfs:domain :Affix ;
                
                rdfs:range :Stem .



###  http://mmoon.org/mmoon/belongsToLexeme

:belongsToLexeme rdf:type owl:ObjectProperty ;
                 
                 rdfs:label "belongs to lexeme"@en ;
                 
                 rdfs:comment "States that a word-form resource belongs to a lexeme resource."@en ;
                 
                 rdfs:range :Lexeme ;
                 
                 rdfs:domain :Wordform ;
                 
                 owl:inverseOf :hasWordform .



###  http://mmoon.org/mmoon/belongsToWord

:belongsToWord rdf:type owl:ObjectProperty ;
               
               rdfs:label "belongs to word"@en ;
               
               rdfs:comment "States that a morph belongs to some word."@en ;
               
               rdfs:domain :Morph ;
               
               rdfs:range :Word ;
               
               owl:inverseOf :consistsOfMorph .



###  http://mmoon.org/mmoon/compoundingRelation

:compoundingRelation rdf:type owl:ObjectProperty ;
                     
                     rdfs:label "States which kind of compounding morphological relationship applies to a compound word."@en ,
                                "compounding morphological relationship"@en ;
                     
                     rdfs:domain :CompoundWord ;
                     
                     rdfs:range :Compounding ;
                     
                     rdfs:subPropertyOf :hasMorphologicalRelationship .



###  http://mmoon.org/mmoon/consistsOfAffix

:consistsOfAffix rdf:type owl:ObjectProperty ;
                 
                 rdfs:label "consists of affix"@en ;
                 
                 rdfs:comment "States that a  word, stem or root consists of some affix."@en ;
                 
                 rdfs:range :Affix ;
                 
                 rdfs:subPropertyOf :consistsOfMorph ;
                 
                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( :Root
                                             :Stem
                                             :Word
                                           )
                             ] .



###  http://mmoon.org/mmoon/consistsOfMorph

:consistsOfMorph rdf:type owl:ObjectProperty ;
                 
                 rdfs:label "consists of morph"@en ;
                 
                 rdfs:comment "States that a word consists of some morph."@en ;
                 
                 rdfs:range :Morph ;
                 
                 rdfs:domain :Word .



###  http://mmoon.org/mmoon/consistsOfMorpheme

:consistsOfMorpheme rdf:type owl:ObjectProperty ;
                    
                    rdfs:label "States that a complex morpheme consists of fusional and/or atomic and/or empty morphemes."@en ,
                               "consist of morpheme"@en ;
                    
                    rdfs:domain :ComplexMorpheme ;
                    
                    rdfs:range [ rdf:type owl:Class ;
                                 owl:unionOf ( :AtomicMorpheme
                                               :EmptyMorpheme
                                               :FusionalMorpheme
                                             )
                               ] .



###  http://mmoon.org/mmoon/consistsOfRoot

:consistsOfRoot rdf:type owl:ObjectProperty ;
                
                rdfs:label "consists of root"@en ;
                
                rdfs:comment "States that a stem or word consists of some root."@en ;
                
                rdfs:range :Root ;
                
                rdfs:subPropertyOf :consistsOfMorph ;
                
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( :Stem
                                            :Word
                                          )
                            ] .



###  http://mmoon.org/mmoon/consistsOfStem

:consistsOfStem rdf:type owl:ObjectProperty ;
                
                rdfs:label "consists of stem"@en ;
                
                rdfs:comment "States that a word or stem consists of some stem."@en ;
                
                rdfs:range :Stem ;
                
                rdfs:subPropertyOf :consistsOfMorph ;
                
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( :Stem
                                            :Word
                                          )
                            ] .



###  http://mmoon.org/mmoon/consistsOfWord

:consistsOfWord rdf:type owl:ObjectProperty ;
                
                rdfs:label "consists of word"@en ;
                
                rdfs:comment "States that an analytic word-form (such as \"more big\" being the comparative form of the adjective \"big\") consists of other word resources."@en ;
                
                rdfs:domain :AnalyticWordform ;
                
                rdfs:range :Word .



###  http://mmoon.org/mmoon/correspondsToMorpheme

:correspondsToMorpheme rdf:type owl:ObjectProperty ;
                       
                       rdfs:label "corresponds to morpheme"@en ;
                       
                       rdfs:comment "States that a given morph corresponds to a certain morpheme."@en ;
                       
                       rdfs:domain :Morph ;
                       
                       rdfs:range :Morpheme .



###  http://mmoon.org/mmoon/derivationalRelation

:derivationalRelation rdf:type owl:ObjectProperty ;
                      
                      rdfs:label "States which kind of derivational morphological relationship applies to a derived word."@en ,
                                 "derivational morphological relationship"@en ;
                      
                      :usageNote "This property is also used to state that a simple lexeme which cannot be further segmented with respect to word-formation has the morphological relationship mmoon:NoWordFormation."@en ;
                      
                      rdfs:range :Derivation ;
                      
                      rdfs:domain :DerivedWord ;
                      
                      rdfs:subPropertyOf :hasMorphologicalRelationship .



###  http://mmoon.org/mmoon/forLanguage

:forLanguage rdf:type owl:ObjectProperty ;
             
             rdfs:label "for language"@en ;
             
             rdfs:comment "States that a MMoOn morpheme inventory is specified for a certain language."@en ;
             
             rdfs:domain :MorphemeInventory ;
             
             rdfs:subPropertyOf :associatedToLanguage .



###  http://mmoon.org/mmoon/hasMeaning

:hasMeaning rdf:type owl:ObjectProperty ;
            
            rdfs:label "has meaning"@en ;
            
            rdfs:range :Meaning ;
            
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( :Morpheme
                                        :Word
                                      )
                        ] .



###  http://mmoon.org/mmoon/hasMorphemicEntry

:hasMorphemicEntry rdf:type owl:ObjectProperty ;
                   
                   rdfs:label "has morphemic entry"@en ;
                   
                   rdfs:comment "States that a morpheme inventory resource has morphemes as morphemic entries, thus building the intension of the morpheme inventory."@en ;
                   
                   rdfs:range :Morpheme ;
                   
                   rdfs:domain :MorphemeInventory .



###  http://mmoon.org/mmoon/hasMorphologicalRelationship

:hasMorphologicalRelationship rdf:type owl:ObjectProperty ;
                              
                              rdfs:label "has morphological relationship"@en ;
                              
                              rdfs:comment "Specifies the morphological relationship of a word."@en ;
                              
                              rdfs:range :MorphologicalRelationship ;
                              
                              rdfs:domain :Word .



###  http://mmoon.org/mmoon/hasRealization

:hasRealization rdf:type owl:ObjectProperty ;
                
                rdfs:label "has realization"@en ;
                
                rdfs:comment "States that a morpheme is realized by some morph."@en ;
                
                rdfs:range :Morph ;
                
                rdfs:domain :Morpheme ;
                
                owl:inverseOf :correspondsToMorpheme .



###  http://mmoon.org/mmoon/hasRepresentation

:hasRepresentation rdf:type owl:ObjectProperty ;
                   
                   rdfs:label "has representation"@en ;
                   
                   rdfs:range :Representation ;
                   
                   rdfs:domain [ rdf:type owl:Class ;
                                 owl:unionOf ( :Morph
                                               :Word
                                             )
                               ] .



###  http://mmoon.org/mmoon/hasWordform

:hasWordform rdf:type owl:ObjectProperty ;
             
             rdfs:label "has wordform"@en ;
             
             rdfs:comment "States that (typically) a lexeme resource has an associated word-form resource."@en ;
             
             rdfs:range :Wordform ;
             
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( :GrammaticalWord
                                         :Lexeme
                                       )
                         ] .



###  http://mmoon.org/mmoon/inflectionalRelation

:inflectionalRelation rdf:type owl:ObjectProperty ;
                      
                      rdfs:label "States which kind of inflectional morphological relationship applies to a word-form."@en ,
                                 "inflectional morphological relationship"@en ;
                      
                      rdfs:range :Inflection ;
                      
                      rdfs:domain :Wordform ;
                      
                      rdfs:subPropertyOf :hasMorphologicalRelationship .



###  http://mmoon.org/mmoon/isAllomorphTo

:isAllomorphTo rdf:type owl:ObjectProperty ,
                        owl:SymmetricProperty ;
               
               rdfs:label "is allomorph to"@en ;
               
               rdfs:comment "States that a morph is an allomorph to another morph which are both realizations of the same morpheme and which occur in complementary or free distribution."@en ;
               
               rdfs:seeAlso """http://www-01.sil.org/linguistics/GlossaryOflinguisticTerms/WhatIsAnAllomorph.htm: 
An allomorph is one of two or more complementary morphs which manifest a morpheme in its different phonological or morphological environments."""@en ;
               
               rdfs:range :Morph ;
               
               rdfs:domain :Morph .



###  http://mmoon.org/mmoon/isComposedOf

:isComposedOf rdf:type owl:ObjectProperty ;
              
              rdfs:label "is composed of"@en ;
              
              rdfs:comment "States of which lexemes or grammatical words the compound word is composed."@en ;
              
              rdfs:domain :CompoundWord ;
              
              rdfs:range [ rdf:type owl:Class ;
                           owl:unionOf ( :GrammaticalWord
                                         :Lexeme
                                       )
                         ] .



###  http://mmoon.org/mmoon/isDerivedFrom

:isDerivedFrom rdf:type owl:ObjectProperty ;
               
               rdfs:label "States from which lexeme or grammatical word the derived word is derived."@en ,
                          "is derived from"@en ;
               
               rdfs:domain :DerivedWord ;
               
               rdfs:range [ rdf:type owl:Class ;
                            owl:unionOf ( :GrammaticalWord
                                          :Lexeme
                                        )
                          ] .



###  http://mmoon.org/mmoon/isHomonymTo

:isHomonymTo rdf:type owl:ObjectProperty ,
                      owl:SymmetricProperty ;
             
             rdfs:label "is homonym to"@en ;
             
             rdfs:comment "States that a morph is homonym to another morph, i.e. although sharing the same orthographic and/or morphemic representation both morphs encode different meanings and thus realize different morphemes."@en ;
             
             rdfs:range :Morph ;
             
             rdfs:domain :Morph .



###  http://mmoon.org/mmoon/partOfLanguage

:partOfLanguage rdf:type owl:ObjectProperty ;
                
                rdfs:label "part of language"@en ;
                
                rdfs:comment "States that a morph or word resources is a linguistic entity as part of a certain language."@en ;
                
                rdfs:subPropertyOf :associatedToLanguage ;
                
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( :Morph
                                            :Word
                                          )
                            ] .





