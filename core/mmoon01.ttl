###  http://mmoon.org/mmoon/consistsOfWord

:consistsOfWord rdf:type owl:ObjectProperty ;
                
                rdfs:label "consists of word"@en ;
                
                rdfs:range :Word .



###  http://mmoon.org/mmoon/correspondsToMorpheme

:correspondsToMorpheme rdf:type owl:ObjectProperty ;
                       
                       rdfs:label "corresponds to morpheme"@en ;
                       
                       rdfs:comment "This property states that a given morph corresponds to a certain morpheme."@en ;
                       
                       rdfs:domain :Morph ;
                       
                       rdfs:range :Morpheme ;
                       
                       owl:inverseOf :hasRealization .



###  http://mmoon.org/mmoon/glossOf

:glossOf rdf:type owl:ObjectProperty ;
         
