                             :Stem .



###  http://mmoon.org/mmoon/consistsOfBase

:consistsOfBase rdf:type owl:ObjectProperty ;
                
                rdfs:label "consists of base"@en ;
                
                rdfs:range :Base ;
                
                rdfs:domain :Base ,
                            :Stem .



###  http://mmoon.org/mmoon/consistsOfMorph

:consistsOfMorph rdf:type owl:ObjectProperty ;
                 
                 rdfs:label "consists of morph"@en ;
                 
                 rdfs:range :Morph ;
                 
                 rdfs:domain :Word .



###  http://mmoon.org/mmoon/consistsOfRoot

:consistsOfRoot rdf:type owl:ObjectProperty ;
                
                rdfs:label "consists of root"@en ;
                
                rdfs:domain :Base ,
                            :Root ;
                
                rdfs:range :Root ;
                
                rdfs:domain :Stem ,
