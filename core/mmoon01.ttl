         rdfs:range :Meaning ;
         
         rdfs:domain :MorphemicGloss .



###  http://mmoon.org/mmoon/hasAbstractIdentity

:hasAbstractIdentity rdf:type owl:ObjectProperty ;
                     
                     rdfs:label "has abstract identity"@en ;
                     
                     rdfs:comment "This property states that a morpheme resource is assigned to one (ore more) morphemic gloss."@en ;
                     
                     rdfs:domain :Morpheme ;
                     
                     rdfs:range :MorphemicGloss .



###  http://mmoon.org/mmoon/hasAgreementFeature

:hasAgreementFeature rdf:type owl:ObjectProperty ;
                     
                     rdfs:label "has agreement feature"@en ;
                     
                     rdfs:range :LinguisticCategory ;
                     
                     rdfs:subPropertyOf :hasMeaning .



###  http://mmoon.org/mmoon/hasConjugationClass

:hasConjugationClass rdf:type owl:ObjectProperty ;
                     
