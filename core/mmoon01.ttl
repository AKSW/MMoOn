                  rdfs:subClassOf :Nominalization ;
                  
                  rdfs:comment "An agent nominalizer is a nominalizer which derives agent nouns. [Christian Lehmann, under \"agent nominalizer\" at http://linguistik.uni-erfurt.de:8080/lido/Lido]"@en .



###  http://mmoon.org/mmoon/AgentNoun

:AgentNoun rdf:type owl:Class ;
           
           rdfs:label "Agent noun"@en ;
           
           rdfs:subClassOf :DeverbalNoun ;
           
