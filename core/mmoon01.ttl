###  http://mmoon.org/mmoon/hasDerivationalMeaning

:hasDerivationalMeaning rdf:type owl:ObjectProperty ;
                        
                        rdfs:label "has derivational meaning"@en ;
                        
                        rdfs:range :DerivationalMeaning ;
                        
                        rdfs:subPropertyOf :hasMeaning .



###  http://mmoon.org/mmoon/hasGrammaticalMeaning

:hasGrammaticalMeaning rdf:type owl:ObjectProperty ;
                       
                       rdfs:label "has grammatical meaning"@en ;
                       
                       rdfs:comment "Structural grammatical features which are primarily or exclusively relevant for grammar."@en ;
                       
