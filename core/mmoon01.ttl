                       rdfs:subPropertyOf :hasMeaning .



###  http://mmoon.org/mmoon/hasInflectionalFeature

:hasInflectionalFeature rdf:type owl:ObjectProperty ;
                        
                        rdfs:label "has inflectional feature"@en ;
                        
                        rdfs:comment "A grammatical aspect that applies to parts of word-forms. An inflectional feature is a category that consists of a set of inflectional values (e.g. ACCUSATIVE being a value of the feature CASE) and whose expression is obligatory for a given stem’s word class. (CL, lido)"@en ;
                        
                        rdfs:range :LinguisticCategory ;
                        
                        rdfs:subPropertyOf :hasMeaning .



###  http://mmoon.org/mmoon/hasMeaning

:hasMeaning rdf:type owl:ObjectProperty ;
            
            rdfs:label "has meaning"@en ;
            
            rdfs:comment "The relation that associates some LinguisticSign with a SemanticUnit. in gold" ;
            
            rdfs:range :Meaning ;
            
            rdfs:domain :Morph ,
                        :Word .



###  http://mmoon.org/mmoon/hasMorphemicEntry

:hasMorphemicEntry rdf:type owl:ObjectProperty ;
                   
                   rdfs:label "has morphemic entry"@en ;
                   
                   rdfs:comment "This property states that a morpheme inventory resource has morphemes as morphemic entries, thus building the intension of the morpheme inventory."@en ;
                   
                   rdfs:range :Morpheme ;
                   
                   rdfs:domain :MorphemeInventory .



###  http://mmoon.org/mmoon/hasMorphemicGloss

:hasMorphemicGloss rdf:type owl:ObjectProperty ;
                   
                   rdfs:label "has morphemic gloss"@en ;
                   
                   rdfs:domain :Meaning ;
                   
                   rdfs:range :MorphemicGloss ;
                   
                   owl:inverseOf :glossOf ;
                   
                   rdfs:subPropertyOf owl:topObjectProperty .



###  http://mmoon.org/mmoon/hasMorphemicRepresention

:hasMorphemicRepresention rdf:type owl:ObjectProperty ;
                          
                          rdfs:label "has morphemic representation"@en ;
                          
                          rdfs:comment "The morphemic form is the canonical form of a Morph in the morpheme inventory and serves as an entry of the morpheme inventory. It consists of a hyphen and the conventional orthographic representation of it even if it has more than one orthographic surface forms. E.g. the genetive Morph \"-s\" in English has two orthographic surface forms, 1) for all words not ending on <s> it is apostrophe and <s> and 2) for all words ending on <s> the apostrophe alone renders the genetive Morph." ;
                          
                          rdfs:domain :Morph ;
                          
                          rdfs:range :MorphemicRepresentation ;
                          
                          rdfs:subPropertyOf :hasRepresentation .



###  http://mmoon.org/mmoon/hasMorphologicalRelationship

:hasMorphologicalRelationship rdf:type owl:ObjectProperty ;
                              
                              rdfs:label "has morphological relationship"@en ;
                              
                              rdfs:range :MorphologicalRelationship ;
                              
                              rdfs:domain :Word .



###  http://mmoon.org/mmoon/hasMorphosyntacticFeature

:hasMorphosyntacticFeature rdf:type owl:ObjectProperty ;
                           
